#. Type: error
#. Description
#: ../common.templates:3
msgid "Failed to retrieve the preconfiguration file"
msgstr "Dështova në shkarkimin e skedarit të parakonfiguruar"

#. Type: error
#. Description
#: ../common.templates:3
msgid ""
"The file needed for preconfiguration could not be retrieved from "
"${LOCATION}. The installation will proceed in non-automated mode."
msgstr ""
"Skedari i duhur për parakonfigurimin nuk mundi të shkarkohej nga "
"$[LOCATION}. Instalimi do vazhdojë në mënyrën manuale."

#. Type: error
#. Description
#: ../common.templates:9
msgid "Failed to load the preconfiguration file"
msgstr "Dështova në ngarkimin e skedarit të parakonfigurimit"

#. Type: error
#. Description
#: ../common.templates:9
msgid ""
"The installer failed to load the preconfiguration file from ${LOCATION}. The "
"file may be corrupt."
msgstr ""
"Instaluesi dështoi në ngarkimin e skedarit të parakonfigurimit nga ${LOCATION}. "
"Skedari mund të jetë i korruptuar."

#. Type: error
#. Description
#: ../common.templates:35
msgid "Failed to run preseeded command"
msgstr "Dështova në ekzekutimin e komandës"

#. Type: error
#. Description
#: ../common.templates:35
msgid ""
"Execution of preseeded command \"${COMMAND}\" failed with exit code ${CODE}."
msgstr ""
"Ekzekutimi i komandës \"${COMMAND}\" dështoi me gabimin ${CODE}."

#. Type: text
#. Description
#. Main menu item

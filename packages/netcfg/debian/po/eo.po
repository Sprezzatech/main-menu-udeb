msgstr "sendrata"

#. Type: text
#. Description
#: ../netcfg-common.templates:155
msgid "Ethernet or Fast Ethernet"
msgstr "Ethernet aŭ Fast Ethernet"

#. Type: text
#. Description
#: ../netcfg-common.templates:159
msgid "Token Ring"
msgstr "Token Ring"

#. Type: text
#. Description
#: ../netcfg-common.templates:163
msgid "USB net"
msgstr "USB net"

#. Type: text
#. Description
#: ../netcfg-common.templates:172
msgid "Serial-line IP"
msgstr "Serial-line IP"

#. Type: text
#. Description

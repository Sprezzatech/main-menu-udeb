msgid "Ethernet or Fast Ethernet"
msgstr "Ethernet atau Fast Ethernet"

#. Type: text
#. Description
#: ../netcfg-common.templates:137
msgid "Token Ring"
msgstr "Token Ring"

#. Type: text
#. Description
#: ../netcfg-common.templates:141

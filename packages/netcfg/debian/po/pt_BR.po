msgid "Serial-line IP"
msgstr "IP Linha-Serial"

#. Type: text
#. Description
#: ../netcfg-common.templates:160
msgid "Parallel-port IP"
msgstr "IP Porta-Paralela"

#. Type: text
#. Description
#: ../netcfg-common.templates:164
msgid "Point-to-Point Protocol"
msgstr "Protocolo Ponto-a-Ponto"

#. Type: text
#. Description
#: ../netcfg-common.templates:168
msgid "IPv6-in-IPv4"
msgstr "IPv6-em-IPv4"

#. Type: text
#. Description
#: ../netcfg-common.templates:172
msgid "ISDN Point-to-Point Protocol"
msgstr "Protocolo ISDN Ponto-a-Ponto"

#. Type: text
#. Description
#: ../netcfg-common.templates:176
msgid "Channel-to-channel"
msgstr "Canal-a-Canal"

#. Type: text
#. Description
#: ../netcfg-common.templates:180
msgid "Real channel-to-channel"
msgstr "Canal-a-Canal real"

#. Type: text
#. Description

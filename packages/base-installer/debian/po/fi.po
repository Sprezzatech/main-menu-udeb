msgstr "Tiedostoa ${SUBST0} ei noudettu etukteen."

#. Type: error
#. Description
#: ../base-installer.templates:106
msgid "Unknown location ${SUBST0}."
msgstr "Tuntematon sijainti ${SUBST0}."

#. Type: error
#. Description
#: ../base-installer.templates:111
msgid "Failed getting Release file ${SUBST0}."
msgstr "Release-tiedoston ${SUBST0} nouto eponnistui."

#. Type: error
#. Description
#: ../base-installer.templates:116
msgid "Invalid Release file: no valid components."
msgstr "Kelvoton Release-tiedosto, ei kelvollisia osia."

#. Type: error
#. Description
#: ../base-installer.templates:121
msgid "Invalid Release file: no entry for ${SUBST0}."
msgstr "Kelvoton Release-tiedosto, ei tietuetta ${SUBST0} -paketille."

#. Type: error
#. Description
#: ../base-installer.templates:126
msgid "Couldn't download ${SUBST0}."
msgstr "Ei voitu noutaa pakettia ${SUBST0}."


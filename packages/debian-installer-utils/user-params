#!/bin/sh
CMDLINE=/proc/cmdline
for item in $(cat $CMDLINE); do
	var="${item%=*}"
	varnoslash="${var##*/*}"
	if [ "$varnoslash" != "" ]; then
		if [ "$item" = "--" ]; then
			inuser=1
			collect=""
		elif [ "$inuser" ]; then
			if [ -z "$collect" ]; then
				collect="$item"
			else
				collect="$collect $item"
			fi
		fi
	fi
done
for word in $collect; do
	echo "$word"
done

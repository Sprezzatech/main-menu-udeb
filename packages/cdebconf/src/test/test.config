#!/bin/sh -e

debug()
{
	echo $* >&2
}

askquestion()
{
	question=$1
	priority=$2
	echo FSET $question seen false
	read ans
	echo INPUT $priority $question
	read ans
	debug "ANS: $ans"
	echo GO
	read ans
	debug "ANS: $ans"
}

echo "SETTITLE test/title"

read ans
debug "ANS: $ans"

echo "REGISTER test/string test/string-register"

read ans
debug "ANS: $ans"

askquestion test/string-register critical

echo "SUBST test/text-subst TEXT  1234567890abcdefghijklmnopqrstuvwxyz1234567890abcdefghijklmnopqrstuvwxyz1234567890abcdefghijklmnopqrstuvwxyz"

read ans
debug "ANS: $ans"

askquestion "test/text-subst" critical

echo "SETTITLE test/title-2"

read ans
debug "ANS: $ans"

for t in boolean multiselect note password select string text; do
	askquestion "test/$t" critical
done

echo STOP
sleep 1
exit 0

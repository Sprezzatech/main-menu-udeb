#. Type: select
#. Choices
#: ../templates:3
msgid "${DISCS}, Finish"
msgstr "${DISCS}, Afslut"

#. Type: select
#. Description
#: ../templates:4
msgid "Disk to partition:"
msgstr "Disk at partitionere:"

#. Type: select
#. Description
#: ../templates:4
msgid "Please choose one of the listed disks, to create partitions on it."
msgstr "Angiv en af de viste diske for at oprette partitioner på den."

#. Type: error
#. Description
#: ../templates:9
msgid "No disk found"
msgstr "Fandt ingen disk"

#. Type: error
#. Description
#: ../templates:9
msgid ""
"Unable to find any disk in your system. Maybe some kernel modules need to be "
"loaded."
msgstr ""
"Kunne ikke finde nogen diske på dit system. Måske skal du indlæse nogle "
"kernemoduler."

#. Type: error
#. Description
#: ../templates:15
msgid "Partitioning error"
msgstr "Partitioneringsfejl"

#. Type: error
#. Description
#: ../templates:15
msgid "Failed to partition the disk ${DISC}."
msgstr "Kunne ikke partitionere disken ${DISC}."

#. Type: text
#. Description
#. Main menu item
#: ../templates:21
msgid "Partition a hard drive"
msgstr "Partitionér en harddisk"

#~ msgid "Please choose which disc you want to partition"
#~ msgstr "Angiv hvilken disk, du ønsker at partitionere"

#~ msgid "Unable to find any discs"
#~ msgstr "Kunne ikke finde nogen diske"

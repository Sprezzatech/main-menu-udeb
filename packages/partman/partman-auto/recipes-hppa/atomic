	$primary{ }
	method{ palo } .

50 1000 130 ext2
	$primary{ }
	method{ format }
	format{ }
	use_filesystem{ }
	filesystem{ ext2 }
        options/noatime{ noatime - do not update inode access times at each access }
	mountpoint{ /boot } .

500 10000 1000000 ext3
	$bootable{ }
	method{ format }
	format{ }
	use_filesystem{ }
	filesystem{ ext3 }
        options/noatime{ noatime - do not update inode access times at each access }
	mountpoint{ / } .

64 512 300% linux-swap
	method{ swap }

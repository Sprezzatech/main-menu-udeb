#. Type: error
#. Description
#: ../templates:3
msgid "Identical mount points for two filesystems"
msgstr "Muntatze puntu berdina bi fitxategi sistementzat"

#. Type: error
#. Description
#: ../templates:3
msgid "Two file systems have to be mounted on ${MOUNTPOINT} simultaneously: ${PART1} and ${PART2}."
msgstr "Bi fitxategi sistema batera  ${MOUNTPOINT}-en muntatuko dira:  ${PART1} eta ${PART2}."

#. Type: error
#. Description
#: ../templates:3
msgid "Please correct this by changing mount points."
msgstr "Konpondu hau muntatze puntuak aldatzen"

#. Type: error
#. Description
#: ../templates:11
msgid "No root file system"
msgstr "Ez dago erro ditxategi sistemarik."

#. Type: error
#. Description
#: ../templates:11
msgid "No root file system is defined."
msgstr "Ez dago erro fitxategi sistemarik ezarririk."

#. Type: error
#. Description
#: ../templates:11
msgid "Please correct this from the partitioning menu."
msgstr "Konpondu ezazu hau partitze menuan."

#. Type: boolean
#. Description
#: ../templates:18
msgid "Do you want to resume partitioning?"
msgstr "Partitzea berrabiaraztea nahi al duzu?"

#. Type: boolean
#. Description
#: ../templates:18
msgid "The attempt to mount a file system with type ${TYPE} in ${DEVICE} at ${MOUNTPOINT} failed."
msgstr "${DEVICE}-ko ${TYPE} motako fitxategi sistema ${MOUNTPOINT}-n muntatzen saiatzerakoan huts egindu."

#. Type: boolean
#. Description
#: ../templates:18
msgid "You may resume partitioning from the partitioning menu."
msgstr "Zuk partitzea berrabiarazi behar duzu partitze menutik."

#. Type: select
#. Description
#: ../templates:27
msgid "Filesystem for this partition:"
msgstr "Partizio honetzako fitxategi sistema:"

#. Type: select
#. Description
#: ../templates:32
msgid "Method for using this partition:"
msgstr "Partizio honentzako erabilitako Metodoa:"

#. Type: text
#. Description
#: ../templates:36
msgid "File system:"
msgstr "Fitxategi sistema"

#. Type: text
#. Description
#: ../templates:40
msgid "Usage method:"
msgstr "Erabilitako metodoa:"

#. Type: text
#. Description
#: ../templates:44
msgid "no file system"
msgstr "ez dago fitxategi sistemarik"


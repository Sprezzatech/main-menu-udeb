#. Type: error
#. Description
#: ../templates:3
msgid "Identical mount points for two filesystems"
msgstr "Enaki priklopni točki za dva datotečna sistema"

#. Type: error
#. Description
#: ../templates:3
msgid ""
"Two file systems have to be mounted on ${MOUNTPOINT} simultaneously: "
"${PART1} and ${PART2}."
msgstr "Dva datotečna sistema želite sočasno priklopiti na {MOUNTPOINT}: ${PART1} in ${PART2}."

#. Type: error
#. Description
#: ../templates:3
msgid "Please correct this by changing mount points."
msgstr "Popravite napako, tako da spremenite priklopni točki."

#. Type: error
#. Description
#: ../templates:11
msgid "No root file system"
msgstr "Ni datotečnega sistema root"

#. Type: error
#. Description
#: ../templates:11
msgid "No root file system is defined."
msgstr "Datotečni sistem root ni definiran."

#. Type: error
#. Description
#: ../templates:11
msgid "Please correct this from the partitioning menu."
msgstr "Popravite napako v meniju za razdelke."

#. Type: boolean
#. Description
#: ../templates:18
msgid "Do you want to resume partitioning?"
msgstr "Želite ponoviti razdeljevanje diska?"

#. Type: boolean
#. Description
#: ../templates:18
msgid ""
"The attempt to mount a file system with type ${TYPE} in ${DEVICE} at "
"${MOUNTPOINT} failed."
msgstr ""
"Poskus priklopa datotečnega sistema vrste ${TYPE} na ${DEVICE} v "
"${MOUNTPOINT} ni uspel."

#. Type: boolean
#. Description
#: ../templates:18
msgid "You may resume partitioning from the partitioning menu."
msgstr "Razdeljevanje lahko ponovite iz menija za razdeljevanje."

#. Type: select
#. Description
#: ../templates:27
msgid "Filesystem for this partition:"
msgstr "Datotečni sistem za ta razdelek:"

#. Type: select
#. Description
#: ../templates:32
msgid "Method for using this partition:"
msgstr "Metoda za uporabo tega razdelka:"

#. Type: text
#. Description
#: ../templates:36
msgid "File system:"
msgstr "Datotečni sistem:"

#. Type: text
#. Description
#: ../templates:40
msgid "Usage method:"
msgstr "Metoda uporabe:"

#. Type: text
#. Description
#: ../templates:44
msgid "no file system"
msgstr "ni datotečnega sistema"


#. Short variant of `swap space'
#: ../partman-basicfilesystems.templates:195
msgid "swap"
msgstr "swap"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:203
msgid "noatime - do not update inode access times at each access"
msgstr "noatime - inode-ok legutbbi elrsi idejnek feljegyzse elmarad"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:203
msgid "nodev - do not support character or block special devices"
msgstr "nodev - specilis (karakteres s blokkos) eszkzfjlok tmogatsnak mellzse"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:203
msgid "nosuid - ignore set-user-identifier or set-group-identifier bits"
msgstr ""
"nosuid - fjlokon tallhat felhasznliazonost- illetve csoportazonost-knyszert "
"bitek figyelmen kvl hagysa"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:203
msgid "noexec - do not allow execution of any binaries"
msgstr "noexec - futtathat binris fjlok futtatsnak tiltsa"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:203
msgid "usrquota - user disk quota accounting enabled"
msgstr "usrquota - felhasznlk lemezhasznlati mrtknek naplzsa"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:203
msgid "grpquota - group disk quota accounting enabled"
msgstr "grpquota - felhasznli csoportok lemezhasznlati mrtknek naplzsa"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:209
msgid "ro - mount the file system read-only"
msgstr "ro - fjlrendszer felcsatlakoztatsa csak olvashat mdban"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:209
msgid "sync - all input/output activities occur synchronously"
msgstr ""
"sync - a fjlrendszeren trtn minden bemeneti/kimeneti mvelet szinkron mdon "
"lesz vgrehajtva"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:209
msgid "quiet - changing owner and permissions does not return errors"
msgstr ""
"quiet - fjlok tulajdonosnak s jogosultsgainak megvltoztatsakor felmerl hibk "
"nem eredmnyeznek hibazeneteket"

#. Type: multiselect
#. Description
#: ../partman-basicfilesystems.templates:210
msgid "Mount options:"
msgstr "Csatlakoztatsi kapcsolk:"

#. Type: multiselect
#. Description
#: ../partman-basicfilesystems.templates:210
msgid "Mount options can tune the behaviour of the file system."
msgstr "A felcsatlakoztatsi kapcsolkkal a fjlrendszer viselkedst hangolhatja."

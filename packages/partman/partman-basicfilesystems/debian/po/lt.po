msgid "noatime - do not update inode access times at each access"
msgstr "noatime - neatnaujint kreipimosi laiko kiekvieno kreipimosi metu"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "nodev - do not support character or block special devices"
msgstr "nodev - neleist kurti speciali simbolini ar blokini rengini"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "nosuid - ignore set-user-identifier or set-group-identifier bits"
msgstr "nosuid - ignoruoti SUID ir SGID bitus"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "noexec - do not allow execution of any binaries"
msgstr "noexec - neleisti vykdyti joki program"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "usrquota - user disk quota accounting enabled"
msgstr "usrquota - leisti naudotoj disko kvot apskait"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "grpquota - group disk quota accounting enabled"
msgstr "grpquota - leisti grupi disko kvot apskait"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "user_xattr - support user extended attributes"
msgstr "user_xattr - iplst naudotojo atribut palaikymas"


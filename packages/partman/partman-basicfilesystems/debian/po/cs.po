#. Type: multiselect
#. Choices
#. Note to translators : Please keep your translations of the choices
#. below a 65 columns limit (which means 65 characters
#. in single-byte languages)
#: ../partman-basicfilesystems.templates:221
msgid "noatime - do not update inode access times at each access"
msgstr "noatime - neaktualizuje as pstupu k souboru"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "nodev - do not support character or block special devices"
msgstr "nodev - zake speciln znakov nebo blokov zazen"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "nosuid - ignore set-user-identifier or set-group-identifier bits"
msgstr "nosuid - ignoruje pznaky setuid a setgid"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "noexec - do not allow execution of any binaries"
msgstr "noexec - nedovol spoutn spustitelnch soubor"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "usrquota - user disk quota accounting enabled"
msgstr "usrquota - povol pouvn diskovch kvt"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "grpquota - group disk quota accounting enabled"
msgstr "grpquota - povol pouvn skupinovch diskovch kvt"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:221
msgid "user_xattr - support user extended attributes"
msgstr "user_xattr - podpora pro uivatelsk rozen atributy"

#. Type: multiselect
#. Choices
#. Note to translators : Please keep your translations of the choices
#. below a 65 columns limit (which means 65 characters
#. in single-byte languages) including the initial path
#: ../partman-basicfilesystems.templates:230
msgid "ro - mount the file system read-only"
msgstr "ro - pipoj souborov systm pouze pro ten"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:230
msgid "sync - all input/output activities occur synchronously"
msgstr "sync - vechny vstupn/vstupn operace se dj synchronn"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:230
msgid "quiet - changing owner and permissions does not return errors"
msgstr "quiet - zmny vlastnka a pstupovch prv nevrt chybu"

#. Type: multiselect
#. Description
#: ../partman-basicfilesystems.templates:231
msgid "Mount options:"
msgstr "Volby pipojen:"

#. Type: multiselect
#. Description
#: ../partman-basicfilesystems.templates:231
msgid "Mount options can tune the behaviour of the file system."
msgstr "Volby pipojen mohou vyladit chovn souborovho systmu."

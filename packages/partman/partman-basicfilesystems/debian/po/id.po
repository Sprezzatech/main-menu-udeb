#  Type: multiselect
#  Choices
#. Type: multiselect
#. Choices
#: ../templates:169
msgid "noatime - do not update inode access times at each access"
msgstr "noatime - jangan memperbaharui catatan waktu setiap kali mengakses inode"

#  Type: multiselect
#  Choices
#. Type: multiselect
#. Choices
#: ../templates:169
msgid "nodev - do not support character or block special devices"
msgstr "nodev - jangan mendukung karakter atau piranti khusus blok"

#  Type: multiselect
#  Choices
#. Type: multiselect
#. Choices
#: ../templates:169
msgid "nosuid - ignore set-user-identifier or set-group-identifier bits"
msgstr "nosuid - abaikan bit-bit set-user-identifier atau set-group-identifier"

#  Type: multiselect
#  Choices
#. Type: multiselect
#. Choices
#: ../templates:169
msgid "noexec - do not allow execution of any binaries"
msgstr "noexec - jangan memperbolehkan eksekusi program biner apapun"

#  Type: multiselect
#  Choices
#. Type: multiselect
#. Choices
#: ../templates:169
msgid "usrquota - user disk quota accounting enabled"
msgstr "usrquota - penghitungan kuota hard disk untuk setiap pengguna diaktifkan"

#  Type: multiselect
#  Choices
#. Type: multiselect
#. Choices
#: ../templates:169
msgid "grpquota - group disk quota accounting enabled"
msgstr "grpquota - penghitungan kuota hard disk untuk setiap grup diaktifkan"

#  Type: multiselect
#  Choices
#. Type: multiselect
#. Choices
#: ../templates:175
msgid "ro - mount the file system read-only"
msgstr "ro - kaitkan sistem berkas sebagai hanya-baca (read-only)"

#  Type: multiselect
#  Choices
#. Type: multiselect
#. Choices
#: ../templates:175
msgid "sync - all input/output activities occur synchronously"
msgstr "sync - semua hal yang berhubungan dengan masukan/keluaran terjadi secara serempak"

#  Type: multiselect
#  Choices
#. Type: multiselect
#. Choices
#: ../templates:175
msgid "quiet - changing owner and permissions does not return errors"
msgstr "quiet - perubahan kepemilikan dan perijinan tidak menghasilkan kesalahan"


msgstr "/boot - קבצים סטטיים של מנהל האתחול"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:76
msgid "/home - user home directories"
msgstr "/home - ספריות הבית של המשתמשים"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:76
msgid "/tmp - temporary files"
msgstr "/tmp - קבצים זמניים"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:76
msgid "/usr - static data"
msgstr "/usr - מידע סטטי"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:76
msgid "/var - variable data"
msgstr "/var - מידע משתנה"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:76
msgid "/srv - data for services provided by this system"
msgstr "/srv - מידע בשביל שירותים המסופקים ע\"י מערכת זאת"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:76
msgid "/opt - add-on application software packages"
msgstr "/opt - תוספות של חבילות תוכנה"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:76
msgid "/usr/local - local hierarchy"
msgstr "/usr/local - הררכיה מקומית"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:81
msgid "/dos"
msgstr "/dos"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:81
msgid "/windows"
msgstr "/windows"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:81
msgid "Enter manually"
msgstr "הזנה באופן ידני"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:81
msgid "Do not mount it"
msgstr "ללא עגינה"

#. Type: string
#. Description
#: ../partman-basicfilesystems.templates:86
msgid "Mount point for this partition:"
msgstr "נקודת עגינה למחיצה זאת:"

#. Type: error
#. Description
#: ../partman-basicfilesystems.templates:90
msgid "Invalid mount point"
msgstr "נקודת עגינה לא חוקית"

#. Type: error
#. Description
#: ../partman-basicfilesystems.templates:90
msgid "The mount point you entered is invalid."
msgstr "נקודת העגינה שהוכנסה אינה חוקית."

#. Type: error
#. Description
#: ../partman-basicfilesystems.templates:90
msgid "Mount points must start with \"/\". They cannot contain spaces."
msgstr "נקודות עגינה חייבות להתחיל בתו  \"/\" ואסור שיכילו רווחים."

#. Type: string
#. Description
#: ../partman-basicfilesystems.templates:97
msgid "Label for the file system in this partition:"
msgstr "תווית למערכת הקבצים במחיצה זאת:"

#. Type: text
#. Description
#: ../partman-basicfilesystems.templates:101
msgid "Format the swap area:"
msgstr "אתחול איזור ה-SWAP:"

#. Type: text
#. Description
#. In the following context: "Format the partition: yes"
#: ../partman-basicfilesystems.templates:106
msgid "yes"
msgstr ""
"#-#-#-#-#  he.po (partman-basicmethods)  #-#-#-#-#\n"
"כן#-#-#-#-#  he.po (partman-basicfilesystems)  #-#-#-#-#\n"
"כן"

#. Type: text
#. Description
#. In the following context: "Format the partition: no"
#: ../partman-basicfilesystems.templates:111
msgid "no"
msgstr ""
"#-#-#-#-#  he.po (partman-basicmethods)  #-#-#-#-#\n"
"לא#-#-#-#-#  he.po (partman-basicfilesystems)  #-#-#-#-#\n"
"לא"


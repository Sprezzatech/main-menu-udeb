#. Choices
#: ../templates:66
msgid "/dos"
msgstr "/dos"

#. Type: select
#. Choices
#: ../templates:66
msgid "/windows"
msgstr "/windows"

#. Type: select
#. Choices
#: ../templates:66
msgid "Enter manually"
msgstr "Inserire manualmente"

#. Type: select
#. Choices
#: ../templates:66
msgid "Do not mount it"
msgstr "Non montarla"

#. Type: string
#. Description
#: ../templates:71
msgid "Mount point for this partition:"
msgstr "Mount point per questa partizione:"

#. Type: error
#. Description
#: ../templates:75
msgid "Invalid mount point"
msgstr "Mount point non valido"

#. Type: error
#. Description
#: ../templates:75
msgid "The mount point you entered is invalid."
msgstr "Il mount point inserito non è valido."

#. Type: error
#. Description
#: ../templates:75
msgid "Mount points must start with \"/\". They cannot contain spaces."
msgstr "I mount point devono iniziare con /. Non possono contente spazi."


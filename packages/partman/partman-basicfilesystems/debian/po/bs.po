#. Choices
#: ../templates:61
msgid "/, /boot, /home, /tmp, /usr, /usr/local, /var, Enter manually, Do not mount it"
msgstr "/, /boot, /home, /tmp, /usr, /usr/local, /var, Unesi ručno, Nemoj montirati"

#. Type: select
#. Choices
#: ../templates:66
msgid "/dos"
msgstr "/dos"

#. Type: select
#. Choices
#: ../templates:66
msgid "/windows"
msgstr "/windows"

#  Type: select
#  Choices
#. Type: select
#. Choices
#: ../templates:66
msgid "Enter manually"
msgstr "Unesi ručno"

#. Type: select
#. Choices
#: ../templates:66
msgid "Do not mount it"
msgstr "Nemoj montirati"

#. Type: string
#. Description
#: ../templates:71
msgid "Mount point for this partition:"
msgstr "Tačka montiranja za ovu particiju:"

#. Type: error
#. Description
#: ../templates:75
msgid "Invalid mount point"
msgstr "Neispravna tačka montiranja"

#. Type: error
#. Description
#: ../templates:75
msgid "The mount point you entered is invalid."
msgstr "Tačka montiranja koju ste unijeli nije ispravna."

#. Type: error
#. Description
#: ../templates:75
msgid "Mount points must start with \"/\". They cannot contain spaces."
msgstr "Tačke montiranja moraju počinjati sa \"/\".  One ne mogu sadržavati razmake."

#. Type: text
#. Description
#: ../templates:82
msgid "Use the partition as a swap area"
msgstr "Koristi partciju kao swap prostor"

#. Type: text
#. Description
#: ../templates:86
msgid "Format the swap area:"
msgstr "Formatiraj swap prostor:"


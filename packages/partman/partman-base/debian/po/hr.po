msgstr "IDE%s gazda, particija br.%s (%s)"

#. Type: text
#. Description
#. For example IDE2 slave, partition #5 (hdf5)
#: ../partman.templates:241
#, no-c-format
msgid "IDE%s slave, partition #%s (%s)"
msgstr "IDE%s rob, particija br.%s (%s)"

#. Type: text
#. Description
#: ../partman.templates:245
#, no-c-format
msgid "SCSI%s (%s,%s,%s) (%s)"
msgstr "SCSI%s (%s,%s,%s) (%s)"

#. Type: text
#. Description
#: ../partman.templates:249
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s (%s)"
msgstr "SCSI%s (%s,%s,%s), particija br.%s (%s)"

#. Type: text
#. Description
#: ../partman.templates:253
#, no-c-format
msgid "RAID%s device #%s"
msgstr "RAID%s ureaj br.%s"

#. Type: text
#. Description
#: ../partman.templates:257
#, no-c-format
msgid "LVM VG %s, LV %s"
msgstr "LVM VG %s, LV %s"

#. Type: text
#. Description
#: ../partman.templates:261
#, no-c-format
msgid "Loopback (loop%s)"
msgstr ""

#. Type: text
#. Description
#: ../partman.templates:265
msgid "Cancel this menu"
msgstr "Otkai ovaj izabirnik"

#. Type: text
#. Description
#. Main menu entry
#: ../partman.templates:270
msgid "Partition disks"
msgstr "Particioniraj diskove"

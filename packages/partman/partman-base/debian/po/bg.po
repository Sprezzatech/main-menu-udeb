#. Type: text
#. Description
#: ../partman-base.templates:54001
#, no-c-format
msgid "DASD %s (%s)"
msgstr "DASD %s (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:55001
#, no-c-format
msgid "DASD %s (%s), partition #%s"
msgstr "DASD %s (%s), дял №%s"

#. Type: text
#. Description
#: ../partman-base.templates:56001
msgid "Cancel this menu"
msgstr "Излизане от това меню"

#. Type: text

#. Description
#. :sl1:
#: ../partman-base.templates:274
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s (%s)"
msgstr "SCSI%s (%s,%s,%s), विभाजन #%s (%s)"

#. Type: text
#. Description
#. :sl3:
#: ../partman-base.templates:279
#, no-c-format
msgid "RAID%s device #%s"
msgstr "RAID%s यन्त्र #%s"

#. Type: text
#. Description
#. :sl3:
#: ../partman-base.templates:284
#, no-c-format

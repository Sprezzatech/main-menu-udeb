#. Type: text
#. Description
#: ../partman-base.templates:265
#, no-c-format
msgid "Loopback (loop%s)"
msgstr "Bukliru (loop%s)"

#. Type: text
#. Description
#: ../partman-base.templates:269
msgid "Cancel this menu"
msgstr "Malvalidigu tiun menuon"

#. Type: text

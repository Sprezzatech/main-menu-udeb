#!/bin/sh

case "`archdetect`" in
	# parted does not properly edit dvh disk labels
	r4k-ip22 | r5k-ip22 | r8k-ip26 | r10k-ip28)
		return 1
		;;
	r10k-ip27 | r12k-ip27)
		return 1
		;;
	r5k-ip32 | r10k-ip32 | r12k-ip32)
		return 1
		;;
	# parted does not understand atari disks
	m68k/atari | m68k/q40)
		return 1
		;;
	*)
		return 0;
esac


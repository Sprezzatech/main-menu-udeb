#: ../templates:109
msgid "Action on the chosen free space:"
msgstr "Gweithred ar y gofod rhydd dewisiedig:"

#. Type: select
#. Description
#: ../templates:114
msgid "Partition settings:"
msgstr "Gosodiadau rhaniad:"

#. Type: select
#. Description
#: ../templates:114
msgid "You are editing partition #${PARTITION} of ${DEVICE}. ${OTHERINFO}"
msgstr ""


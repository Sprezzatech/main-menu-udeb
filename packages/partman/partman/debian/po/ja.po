#: ../templates:216
#, no-c-format
msgid "IDE%s master, partition #%s"
msgstr "IDE%s マスタ パーティション %s."

#. Type: text
#. Description
#. For example IDE2 slave, partition #5
#: ../templates:221
#, no-c-format
msgid "IDE%s slave, partition #%s"
msgstr "IDE%s スレーブ パーティション %s."

#. Type: text
#. Description
#: ../templates:225
#, no-c-format
msgid "SCSI%s (%s,%s,%s)"
msgstr "SCSI%s (%s,%s,%s)"

#. Type: text
#. Description
#: ../templates:229
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s"
msgstr "SCSI%s (%s,%s,%s) パーティション %s."

#. Type: text
#. Description
#: ../templates:233
msgid "Cancel this menu"
msgstr "このメニューのキャンセル"

#. Type: text
#. Description
#. Main menu entry
#: ../templates:238
msgid "Partition disks"
msgstr "ディスクのパーティショニング"


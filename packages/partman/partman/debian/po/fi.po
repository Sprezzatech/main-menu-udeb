#. Type: text
#. Description
#. For example IDE2 slave, partition #5
#: ../templates:221
#, no-c-format
msgid "IDE%s slave, partition #%s"
msgstr "IDE%s slave, levyosio n:ro %s"

#. Type: text
#. Description
#: ../templates:225
#, no-c-format
msgid "SCSI%s (%s,%s,%s)"
msgstr "SCSI%s (%s,%s,%s)"

#. Type: text

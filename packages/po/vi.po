msgstr "Giao thức điểm-đến-điểm (PPP)"

#. Type: text
#. Description
#: ../netcfg-common.templates:196
msgid "IPv6-in-IPv4"
msgstr "IPv6-trong-IPv4"

#. Type: text
#. Description
#: ../netcfg-common.templates:200
msgid "ISDN Point-to-Point Protocol"
msgstr "Giao thức điểm-đến điểm ISDN"

#. Type: text
#. Description
#: ../netcfg-common.templates:204
msgid "Channel-to-channel"
msgstr "Kênh-đến-kênh"

#. Type: text
#. Description
#: ../netcfg-common.templates:208
msgid "Real channel-to-channel"
msgstr "Kênh-đến-kênh thật"

#. Type: text

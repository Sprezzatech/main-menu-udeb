msgstr "-- Asya --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Afrîka --"

#: ../../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- Okeanûsa Hînd"

#: ../../countrylist:22
msgid "-- Oceania --"
msgstr "-- Okeyaniya --"

#: ../../countrylist:23
msgid "-- Antarctica --"
msgstr "-- Antarktîk --"

#: ../../countrylist:24
msgid "-- other --"
msgstr "-- wekî din --"

#. Type: text
#. Description
#. Main menu item. Please keep below 55 columns
#: ../kbd-chooser.templates-in:4
msgid "Select a keyboard layout"
msgstr "Rengê klavyeyê hilbijêre"

#. Type: select
#. Description
#: ../kbd-chooser.templates-in:10
msgid "Type of keyboard:"
msgstr "Cureyê klavyeyê:"

#. Type: select
#. Description
#: ../kbd-chooser.templates-in:10

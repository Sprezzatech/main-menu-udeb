msgid "KEYSTROKES:"
msgstr "BIŞKOK:"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:14
msgid "Display this help message"
msgstr "Vê peyama alîkariyê nîşan bide"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:19
msgid "Go back to previous question"
msgstr "Vegere pirsa berê"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:24
msgid "Select an empty entry"
msgstr "Nirxeke vala ji bo têketinê hilbijêre"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:39
#, no-c-format
msgid "Prompt: '%c' for help, default=%d> "
msgstr "Daxwaz: Ji bo alîkariyê '%c', heyî=%d> "

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:44
#, no-c-format
msgid "Prompt: '%c' for help> "
msgstr "Daxwaz: ji bo alikariyê '%c'> "

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:49
#, no-c-format
msgid "Prompt: '%c' for help, default=%s> "
msgstr "Daxwaz: ji bo alîkariyê '%c', heyî=%s> "

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:54
msgid "[Press enter to continue]"
msgstr "[Ji bo domandinê: enter]"

#. Type: text
#. Description
#. Main menu item
#. should not be more than 55 columns
#. pkgsel is the module that installs packages by running tasksel to
#. select "tasks". Please use "install *software*" and not
#. "install *packages*" which is less adapted for non technical users
#: ../pkgsel.templates:8
msgid "Select and install software"
msgstr "Nivîsbariyê hilbijêre û saz bike"

#. Type: text
#. Description
#. This appears in a progress bar when running pkgsel
#. The text is used when pkgsel is launched, before it installs packages
#: ../pkgsel.templates:14
msgid "Setting up..."
msgstr "Tê sazkirin..."

#. Type: text
#. Description
#. This appears in a progress bar when running pkgsel
#. The text is used when running tasksel to allow selecting packages
#. Tasksel will then display its own screens
#: ../pkgsel.templates:21
msgid "Running tasksel..."
msgstr "tasksel tê meşandin..."

#. Type: text
#. Description
#. This appears in a progress bar when running pkgsel
#. The text is used at the end of the installation phase while
#. cleaning up pkgsel's stuff
#: ../pkgsel.templates:28
msgid "Cleaning up..."
msgstr "Tê paqijkirin..."

#. Type: text
#. Description
#. Main menu item
#. :sl1:
#: ../finish-install.templates:5
msgid "Finish the installation"
msgstr "Sazkirinê bidawî bide"

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:10
msgid "Finishing the installation"
msgstr "Sazkirin tê temamkirin"

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:20
msgid "Configuring network..."
msgstr "Tor tê avakirin..."

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:25
msgid "Setting up frame buffer..."
msgstr "Frame buffer tê sazkirin..."

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:30
msgid "Unmounting file systems..."
msgstr "Pergalên pelan tên unmountkirin..."

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:35
msgid "Rebooting into your new system..."
msgstr "Pergala te ya nû boot dike..."

#. Type: note
#. Description
#. :sl1:
#: ../finish-install.templates:40
msgid "Installation complete"
msgstr "Sazkirin temam e"

#. Type: note
#. Description
#. :sl1:
#: ../finish-install.templates:40

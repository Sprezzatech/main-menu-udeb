msgid "Display this help message"
msgstr "Prikaži to besedilo"

#. Type: text
#. Description
#: ../cdebconf-text-udeb.templates:15
msgid "Go back to previous question"
msgstr "Pojdi nazaj na prejšnje vprašanje"

#. Type: text
#. Description
#: ../cdebconf-text-udeb.templates:19
msgid "Select an empty entry"
msgstr "Izberi prazen niz"

#. Type: text
#. Description
#: ../cdebconf-text-udeb.templates:31
#, no-c-format
msgid "Prompt: '%c' for help, default=%d> "
msgstr "Poziv: '%c' za pomoč, privzeto=%d> "

#. Type: text
#. Description
#: ../cdebconf-text-udeb.templates:35
#, no-c-format

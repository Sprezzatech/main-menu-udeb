#. Type: text
#. Description
#: ../keyboard-sparc.templates:3
msgid "Sun keyboard"
msgstr "Teclado Sun"

#. Type: text
#. Description
#: ../keyboard-usb.templates:3
msgid "USB keyboard"
msgstr "Teclado USB"

#. Type: text
#. Description
#: ../keyboard-dec.templates:3
msgid "DEC keyboard"
msgstr "Teclado DEC"

#. Type: text
#. Description
#: ../keyboard-hil.templates:3
msgid "HP HIL keyboard"

msgstr "Desa els registres de depuració"

#. Type: select
#. Choices
#. Possible locations for debug logs to be saved
#: ../save-logs.templates:9
msgid "floppy"
msgstr "disquet"

#. Type: select
#. Choices
#. Possible locations for debug logs to be saved
#: ../save-logs.templates:9
msgid "web"
msgstr "web"

#. Type: select

#. Description
#. :sl6:
#: ../cdebconf.templates:2001
msgid "None"
msgstr "Batez"

#. Type: string
#. Description
#. :sl6:
#: ../cdebconf.templates:2001
msgid "'None' will never ask you any question."
msgstr "'Batez' hautatuaz inoiz ez dizu galderarik egingo."

#. Type: string
#. Description
#. :sl6:
#: ../cdebconf.templates:3001
msgid "Text"
msgstr "Testua"

#. Type: string
#. Description
#. :sl6:
#: ../cdebconf.templates:3001

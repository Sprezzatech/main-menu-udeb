msgstr ""
" interfície         = ${interface}\n"
" adreça IP          = ${ipaddress}\n"
" màscara de xarxa   = ${netmask}\n"
" pasarel·la         = ${gateway}\n"
" punt a punt        = ${pointtopoint}\n"
" servidors de noms  = ${nameservers}"


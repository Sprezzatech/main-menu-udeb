msgstr "Fullfører installasjonen"

#. Type: text
#. Description
#: ../prebaseconfig.templates:8
msgid "Finishing the installation"
msgstr "Fullfører installasjonen"

#. Type: text
#. Description
#: ../prebaseconfig.templates:16
msgid "Configuring network..."
msgstr "Set opp nettverk ..."

#. Type: text
#. Description
#: ../prebaseconfig.templates:20
msgid "Setting up frame buffer..."
msgstr "Set opp skjermbuffer ..."

#. Type: text
#. Description
#: ../prebaseconfig.templates:24
msgid "Saving debconf database..."
msgstr "Lagrar debconf-databasen ..."

#. Type: text
#. Description
#: ../prebaseconfig.templates:28
msgid "Preparing for base-config..."
msgstr "Gjer klart for base-config ..."

#. Type: text
#. Description
#: ../prebaseconfig.templates:32

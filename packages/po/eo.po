#. :sl3:
#: ../partman-base.templates:49001
#, no-c-format
msgid "Serial ATA RAID %s (partition #%s)"
msgstr "Seria 'ATA RAID'-o %s (diskpartigo #%s)"

#. Type: text
#. Description
#. :sl3:
#: ../partman-base.templates:50001
#, no-c-format
msgid "LVM VG %s, LV %s"
msgstr "LVM VG %s, LV %s"

#. Type: text
#. Description
#. :sl3:
#: ../partman-base.templates:51001
#, no-c-format
msgid "Loopback (loop%s)"
msgstr "Bukliru (loop%s)"

#. Type: text

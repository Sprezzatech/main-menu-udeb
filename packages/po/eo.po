msgstr "-- Sud-Amerika --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Kariba --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- Eŭropa --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- Azia --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Afrika --"

#: ../../countrylist:21
msgid "-- Atlantic Ocean --"
msgstr "-- Atlantika --"

#: ../../countrylist:22
msgid "-- Indian Ocean --"
msgstr "-- Indi-Oceana --"

#: ../../countrylist:23

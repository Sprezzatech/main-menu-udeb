msgid "Save debug logs"
msgstr "Lagre feilsøkingslogger"

#. Type: select
#. Choices
#. Possible locations for debug logs to be saved
#: ../save-logs.templates:9
msgid "floppy"
msgstr "diskett"

#. Type: select
#. Choices
#. Possible locations for debug logs to be saved
#: ../save-logs.templates:9
msgid "web"
msgstr "nett"

#. Type: select
#. Choices

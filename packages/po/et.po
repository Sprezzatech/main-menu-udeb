msgid "Token Ring"
msgstr "Token Ring"

#. Type: text
#. Description
#: ../netcfg-common.templates:163
msgid "Arcnet"
msgstr "Arcnet"

#. Type: text
#. Description
#: ../netcfg-common.templates:167
msgid "Serial-line IP"
msgstr "Serial-line IP"

#. Type: text
#. Description

msgid "Token Ring"
msgstr "Token Ring"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:24001
msgid "USB net"
msgstr "רשת USB"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:26001
msgid "Serial-line IP"
msgstr "Serial-line IP"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:27001
msgid "Parallel-port IP"
msgstr "Parallel-port IP"

#. Type: text

msgid "Display this help message"
msgstr "Amosar esta mensaxe de axuda"

#. Type: text
#. Description
#: ../cdebconf-text-udeb.templates:15
msgid "Go back to previous question"
msgstr "Voltar á pregunta anterior"

#. Type: text
#. Description
#: ../cdebconf-text-udeb.templates:19
msgid "Select an empty entry"
msgstr "Seleccionar unha entrada baleira"

#. Type: text
#. Description
#: ../cdebconf-text-udeb.templates:31
#, no-c-format
msgid "Prompt: '%c' for help, default=%d> "
msgstr "Indicativo: '%c' para axuda, por defecto=%d> "

#. Type: text

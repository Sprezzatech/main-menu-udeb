" interface     = ${interface}\n"
" ipaddress     = ${ipaddress}\n"
" netmask       = ${netmask}\n"
" gateway       = ${gateway}\n"
" pointopoint   = ${pointopoint}\n"
" nameservers   = ${nameservers}"
msgstr ""
"انٹرفیس    ={interface}$ ؎ٓای پی اڈریس     ={ipaddress}$نیٹماسک       ="
"{netmask}$گیٹوے      ={gateway}$پوانٹ سے پوانٹ   ={pointopoint}$سرورز کے "
"نام   ={nameservers}$"


msgid "Ethernet or Fast Ethernet"
msgstr "Ethernet alebo Fast Ethernet"

#. Type: text
#. Description
#: ../netcfg-common.templates:171
msgid "Token Ring"
msgstr "Token Ring"

#. Type: text
#. Description
#: ../netcfg-common.templates:175
msgid "USB net"
msgstr "USB sieť"

#. Type: text
#. Description
#: ../netcfg-common.templates:184
msgid "Serial-line IP"
msgstr "Serial-line IP"

#. Type: text
#. Description

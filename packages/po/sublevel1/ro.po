#: ../../countrylist:22
msgid "-- Oceania --"
msgstr "-- Oceania --"

#: ../../countrylist:23
msgid "-- Antarctica --"
msgstr "-- Antarctica --"

#: ../../countrylist:24
msgid "-- other --"
msgstr "-- alta --"

#. Type: text
#. Description
#. Main menu item. Please keep below 55 columns
#: ../kbd-chooser.templates-in:4

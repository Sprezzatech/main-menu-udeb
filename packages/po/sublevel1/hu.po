msgstr "-- Afrika --"

#: ../../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- Indiai-óceán --"

#: ../../countrylist:22
msgid "-- Oceania --"
msgstr "-- Óceánia --"

#: ../../countrylist:23
msgid "-- Antarctica --"
msgstr "-- Antarktisz --"

#: ../../countrylist:24
msgid "-- other --"
msgstr "-- egyéb --"

#. Type: text

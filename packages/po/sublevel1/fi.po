msgstr "-- Pohjois-Amerikka --"

#: ../../countrylist:15
msgid "-- Central America --"
msgstr "-- Väli-Amerikka --"

#: ../../countrylist:16
msgid "-- South America --"
msgstr "-- Etelä-Amerikka --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Karibia --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- Eurooppa --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- Aasia --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Afrikka --"

#: ../../countrylist:21
msgid "-- Indian Ocean --"

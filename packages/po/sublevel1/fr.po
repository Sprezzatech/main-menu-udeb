#: ../../countrylist:22
msgid "-- Oceania --"
msgstr "-- Océanie --"

#: ../../countrylist:23
msgid "-- Antarctica --"
msgstr "-- Antarctique --"

#: ../../countrylist:24
msgid "-- other --"
msgstr "-- Autre --"

#. Type: text

msgid "-- Europe --"
msgstr "— Châu Âu —"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "— Châu Á —"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "— Châu Phi —"

#: ../../countrylist:21
msgid "-- Indian Ocean --"
msgstr "— Ấn Độ Dương —"

#: ../../countrylist:22
msgid "-- Oceania --"
msgstr "— Úc và Niu Xi-lan —"

#: ../../countrylist:23
msgid "-- Antarctica --"
msgstr "— Nam Cực —"

#: ../../countrylist:24
msgid "-- other --"

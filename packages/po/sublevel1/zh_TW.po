msgstr "Atari 鍵盤"

#. Type: text
#. Description
#. Translators, this is a menu choice. MUST BE UNDER 65 COLUMNS
#: ../keyboard-amiga.templates:4
msgid "Amiga keyboard"
msgstr "Amiga 鍵盤"

#. Type: text
#. Description
#. Translators, this is a menu choice. MUST BE UNDER 65 COLUMNS
#: ../keyboard-acorn.templates:4
msgid "Acorn keyboard"
msgstr "Acorn 鍵盤"

#. Type: text
#. Description
#. Translators, this is a menu choice. MUST BE UNDER 65 COLUMNS
#: ../keyboard-mac.templates:4
msgid "Mac keyboard"
msgstr "Mac 鍵盤"

#. Type: text
#. Description
#. Translators, this is a menu choice. MUST BE UNDER 65 COLUMNS
#: ../keyboard-sparc.templates:4
msgid "Sun keyboard"
msgstr "Sun 鍵盤"

#. Type: text
#. Description
#. Translators, this is a menu choice. MUST BE UNDER 65 COLUMNS
#: ../keyboard-usb.templates:4
msgid "USB keyboard"
msgstr "USB 鍵盤"

#. Type: text
#. Description
#. Translators, this is a menu choice. MUST BE UNDER 65 COLUMNS
#: ../keyboard-dec.templates:4

msgstr "Kernel-images worden gekopieerd naar ${BOOTDEV}..."

#. Type: text
#. Description
#: ../aboot-installer.templates:97
msgid "Aboot boot partition"
msgstr "Aboot-opstartpartitie"

#. Type: text
#. Description
#: ../aboot-installer.templates:101
msgid "aboot"
msgstr "aboot"

#. Type: text
#. Description
#: ../vmelilo-installer.templates:3
msgid "Installing vmelilo"
msgstr "vmelilo wordt geïnstalleerd"

#. Type: text
#. Description
#: ../vmelilo-installer.templates:7
msgid "Installing vmelilo boot loader"
msgstr "vmelilo-opstartlader wordt geïnstalleerd"

#. Type: boolean
#. Description
#: ../vmelilo-installer.templates:12

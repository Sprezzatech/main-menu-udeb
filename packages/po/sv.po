msgid "-- North America --"
msgstr "-- Nordamerika --"

#: ../../countrylist:15
msgid "-- Central America --"
msgstr "-- Centralamerika --"

#: ../../countrylist:16
msgid "-- South America --"
msgstr "-- Sydamerika --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Västindien --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- Europa --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- Asien --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Afrika --"

#: ../../countrylist:21
msgid "-- Atlantic Ocean --"
msgstr "-- Atlantiska oceanen --"

#: ../../countrylist:22
msgid "-- Indian Ocean --"
msgstr "-- Indiska oceanen --"

#: ../../countrylist:23
msgid "-- Oceania --"
msgstr "-- Oceanien --"

#: ../../countrylist:24
msgid "-- Antarctica --"
msgstr "-- Antarktis --"

#: ../../countrylist:25

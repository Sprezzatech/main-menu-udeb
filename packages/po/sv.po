msgid "Screenshot"
msgstr "Skärmdump"

#. Type: text
#. Description
#. Text that will appear in a dialog box mentioning which file
#. the screenshot has been saved to. "%s" is a file name here
#. :sl1:
#: ../cdebconf-gtk-udeb.templates:7001
#, no-c-format
msgid "Screenshot saved as %s"
msgstr "Skärmdump sparad som %s"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:1001
#, no-c-format
msgid "!! ERROR: %s"
msgstr "!! FEL: %s"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:2001
msgid "KEYSTROKES:"
msgstr "NEDSLAG:"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:3001
msgid "Display this help message"
msgstr "Visa det här hjälpmeddelandet"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:4001
msgid "Go back to previous question"
msgstr "Gå tillbaka till föregående fråga"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:5001
msgid "Select an empty entry"
msgstr "Välj en tom post"

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:8001
#, no-c-format
msgid "Prompt: '%c' for help, default=%d> "
msgstr "Val: \"%c\" för hjälp, standardvärde=%d> "

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:9001
#, no-c-format
msgid "Prompt: '%c' for help> "
msgstr "Val: \"%c\" för hjälp> "

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:10001
#, no-c-format
msgid "Prompt: '%c' for help, default=%s> "
msgstr "Val: \"%c\" för hjälp, standardvärde=%s> "

#. Type: text
#. Description
#. :sl4:
#: ../cdebconf-text-udeb.templates:11001
msgid "[Press enter to continue]"
msgstr "[Tryck enter för att fortsätta]"

#. Type: text
#. Description
#. Main menu item
#. should not be more than 55 columns
#. pkgsel is the module that installs packages by running tasksel to
#. select "tasks". Please use "install *software*" and not
#. "install *packages*" which is less adapted for non technical users
#: ../pkgsel.templates:1001
msgid "Select and install software"
msgstr "Välj och installera programvara"

#. Type: text
#. Description
#. This appears in a progress bar when running pkgsel
#. The text is used when pkgsel is launched, before it installs packages
#: ../pkgsel.templates:2001
msgid "Setting up..."
msgstr "Ställer in..."

#. Type: text
#. Description
#. This appears in a progress bar when running pkgsel
#. The text is used when running tasksel to allow selecting packages
#. Tasksel will then display its own screens
#: ../pkgsel.templates:3001
msgid "Running tasksel..."
msgstr "Kör tasksel..."

#. Type: text
#. Description
#. This appears in a progress bar when running pkgsel
#. The text is used at the end of the installation phase while
#. cleaning up pkgsel's stuff
#: ../pkgsel.templates:4001
msgid "Cleaning up..."
msgstr "Rensar upp..."

#. Type: text
#. Description
#. Main menu item
#. :sl1:
#: ../finish-install.templates:1001
msgid "Finish the installation"
msgstr "Slutför installationen"

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:2001
msgid "Finishing the installation"
msgstr "Slutför installationen"

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:4001
msgid "Configuring network..."
msgstr "Konfigurerar nätverket..."

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:5001
msgid "Setting up frame buffer..."
msgstr "Konfigurerar grafikkort..."

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:6001
msgid "Unmounting file systems..."
msgstr "Avmonterar filsystem..."

#. Type: text
#. Description
#. :sl1:
#: ../finish-install.templates:7001
msgid "Rebooting into your new system..."
msgstr "Startar om ditt nya system..."


dhcp=$(remove_xml $(grep "<dhcpenabled>" $TEST_DIR/mv2120_dhcp.nas_conf_db.xml))
if [ "$dhcp" == "true" ]; then
	NET_CONFIG=dhcp
else
	NET_CONFIG=static
	IPADDRESS=$(remove_xml $(grep "<ipaddr>" $TEST_DIR/mv2120_dhcp.nas_conf_db.xml))
	NETMASK=$(remove_xml $(grep "<netmask>" $TEST_DIR/mv2120_dhcp.nas_conf_db.xml))
	GATEWAY=$(remove_xml $(grep "<gateway>" $TEST_DIR/mv2120_dhcp.nas_conf_db.xml))
fi
HOSTNAME=$(remove_xml $(grep "<hostname>" $TEST_DIR/mv2120_dhcp.nas_conf_db.xml | head -n 1))
unset_matching_var "HOSTNAME" "HPMediaVault"
DOMAIN=$(remove_xml $(grep "<domain>" $TEST_DIR/mv2120_dhcp.nas_conf_db.xml))
unset_matching_var "DOMAIN" "local"
if [ "$NET_CONFIG" != "static" ]; then
	IPADDRESS=192.168.1.100
	NETMASK=255.255.255.0
	GATEWAY=192.168.1.1
	[ -z "$NAMESERVERS" ] && NAMESERVERS=192.168.1.1
	dhcp_fallback $1
fi
generate_preseed_file "$1"

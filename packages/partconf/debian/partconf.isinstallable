#!/bin/sh

case "`archdetect`" in
        # sb1-swarm-bn uses partman but due to a bug in main-menu
        # partconf is called after partman.  waldi said he won't have
        # time to fix this for sarge, so put in this workaround.
        mips/sb1-swarm-bn)
                return 1
                ;;
        *)
                return 0;
esac


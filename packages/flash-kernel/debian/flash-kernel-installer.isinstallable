#!/bin/sh
set -e

case "`archdetect`" in
	arm*/iop32x)
		exit 0
	;;
	arm*/ixp4xx)
		exit 0
	;;
	arm*/kirkwood)
		machine=$(grep "^Hardware" /proc/cpuinfo | sed 's/Hardware\s*:\s*//')
		case "$machine" in
			"QNAP TS-119/TS-219")
				exit 0
			;;
			*)
				exit 1
			;;
		esac
	;;
	arm*/orion5x)
		exit 0
	;;
	# Don't activate it by default
	*)
		exit 1
	;;
esac


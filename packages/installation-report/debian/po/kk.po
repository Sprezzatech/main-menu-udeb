#. Type: text
#. Description
#. Main menu item
#: ../save-logs.templates:4
msgid "Save debug logs"
msgstr "Журнал файлдарын жөндеуші информациямен сақтау"

#. Type: select
#. Choices
#. Possible locations for debug logs to be saved
#: ../save-logs.templates:9
msgid "floppy"
msgstr ""

#. Type: select
#. Choices
#. Possible locations for debug logs to be saved
#: ../save-logs.templates:9
msgid "web"
msgstr ""


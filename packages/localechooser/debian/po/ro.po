msgid "-- North America --"
msgstr "-- America de nord --"

#: ../../countrylist:15
msgid "-- Central America --"
msgstr "-- America centrală --"

#: ../../countrylist:16
msgid "-- South America --"
msgstr "-- America de sud --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Caraibe --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- Europa --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- Asia --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Africa --"

#: ../../countrylist:21
msgid "-- Atlantic Ocean --"
msgstr "-- Oceanul Atlantic --"

#: ../../countrylist:22
msgid "-- Indian Ocean --"
msgstr "-- Oceanul Indian --"

#: ../../countrylist:23
msgid "-- Oceania --"
msgstr "-- Oceania --"

#: ../../countrylist:24
msgid "-- Antarctica --"
msgstr "-- Antarctica --"


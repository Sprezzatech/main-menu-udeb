msgid "-- North America --"
msgstr "-- Ziemeļamerika --"

#: ../../countrylist:15
msgid "-- Central America --"
msgstr "-- Centrālamerika --"

#: ../../countrylist:16
msgid "-- South America --"
msgstr "-- Dienvidamērika --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Karību baseins --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- Eiropa --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- Āzija --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Āfrika --"

#: ../../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- Indijas Okeāns --"

#: ../../countrylist:22
msgid "-- Oceania --"
msgstr "-- Okeānija --"

#: ../../countrylist:23
msgid "-- Antarctica --"
msgstr "-- Antarktika --"

#: ../../countrylist:24
msgid "-- other --"
msgstr "-- cita --"

msgid "-- North America --"
msgstr "-- Amérique du Nord --"

#: ../../countrylist:15
msgid "-- Central America --"
msgstr "-- Amérique Centrale --"

#: ../../countrylist:16
msgid "-- South America --"
msgstr "-- Amérique du Sud --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Caraïbes --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- Europe --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- Asie --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Afrique --"

#: ../../countrylist:21
msgid "-- Atlantic Ocean --"
msgstr "-- Océan Atlantique --"

#: ../../countrylist:22
msgid "-- Indian Ocean --"
msgstr "-- Océan Indien --"

#: ../../countrylist:23
msgid "-- Oceania --"
msgstr "-- Océanie --"

#: ../../countrylist:24
msgid "-- Antarctica --"
msgstr "-- Antarctique --"


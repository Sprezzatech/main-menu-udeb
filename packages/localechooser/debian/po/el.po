msgstr "-- Κεντρική Αμερική --"

#: ../../countrylist:16
msgid "-- South America --"
msgstr "-- Νότια Αμερική --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Καραϊβική --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- Ευρώπη --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- Ασία --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Aφρική --"

#: ../../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- Ινδικός Ωκεανός --"

#: ../../countrylist:22
msgid "-- Oceania --"
msgstr "-- Ωκεανία --"

#: ../../countrylist:23

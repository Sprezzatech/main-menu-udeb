msgid "-- Asia --"
msgstr "-- Азия --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Африка --"

#: ../../countrylist:21
msgid "-- Atlantic Ocean --"
msgstr "-- Атлантический океан --"

#: ../../countrylist:22
msgid "-- Indian Ocean --"
msgstr "-- Индийский океан --"

#: ../../countrylist:23
msgid "-- Oceania --"
msgstr "-- Океания --"


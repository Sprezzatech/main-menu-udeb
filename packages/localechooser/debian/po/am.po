msgstr "-- ደቡብ አሜሪካ --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- ካሪቢያን --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- አውሮፓ --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- አሲያ --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- አፍሪካ --"

#: ../../countrylist:21
msgid "-- Atlantic Ocean --"
msgstr "-- አትላንቲክ ውቂያኖስ --"

#: ../../countrylist:22
msgid "-- Indian Ocean --"
msgstr "-- ህንድ ውቂያኖስ --"

#: ../../countrylist:23
msgid "-- Oceania --"
msgstr "-- ኦሲንያ --"


msgid "-- North America --"
msgstr "-- អាមេរិក​ខាង​ជើង​ --"

#: ../countrylist:15
msgid "-- Central America --"
msgstr "-- អាមេរិក​កណ្តាល​ --"

#: ../countrylist:16
msgid "-- South America --"
msgstr "-- អាមេរិ​ខាង​ត្បូង​ --"

#: ../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Caribbean --"

#: ../countrylist:18
msgid "-- Europe --"
msgstr "-- អឺរ៉ុប​ --"

#: ../countrylist:19
msgid "-- Asia --"
msgstr "--អាស៊ី --"

#: ../countrylist:20
msgid "-- Africa --"
msgstr "-- អា​ហ្រិក​ --"

#: ../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- សមុទ្រ​ឥណ្ឌា --"

#: ../countrylist:22
msgid "-- Oceania --"
msgstr "-- ហ្វូង​កោះ --"

#: ../countrylist:23
msgid "-- Antarctica --"
msgstr "-- អង់តាក់​តិច --"

#: ../countrylist:24
msgid "-- other --"

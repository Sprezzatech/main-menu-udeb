msgid "-- Central America --"
msgstr "-- Ameryka Środkowa --"

#: ../countrylist:16
msgid "-- South America --"
msgstr "-- Ameryka Południowa --"

#: ../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Karaiby --"

#: ../countrylist:18
msgid "-- Europe --"
msgstr "-- Europa --"

#: ../countrylist:19
msgid "-- Asia --"
msgstr "-- Azja --"

#: ../countrylist:20
msgid "-- Africa --"
msgstr "-- Afryka --"

#: ../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- Ocean Indyjski --"

#: ../countrylist:22
msgid "-- Oceania --"
msgstr "-- Oceania --"

#: ../countrylist:23
msgid "-- Antarctica --"
msgstr "-- Antarktyka --"

#: ../countrylist:24

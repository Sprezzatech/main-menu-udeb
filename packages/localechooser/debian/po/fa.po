msgid "-- Central America --"
msgstr "--  آمریکای مرکزی --"

#: ../../countrylist:16
msgid "-- South America --"
msgstr "-- آمریکای جنوبی --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- کارائیب --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- اروپا --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- آسیا --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- آفریقا --"

#: ../../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- اقیانوس هند --"

#: ../../countrylist:22
msgid "-- Oceania --"
msgstr "-- اقیانوسیه --"


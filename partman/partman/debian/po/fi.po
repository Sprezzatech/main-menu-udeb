#. Type: select
#. Description
#: ../templates:15
msgid "Partition, free space or device to be modified:"
msgstr "Osio, vapaa tila tai laite jota muokataan:"

#. Type: select
#. Description
#: ../templates:20
msgid "Action on the chosen device:"
msgstr "Valitulle laitteelle tehtävä toimenpide:"

#. Type: select
#. Description
#: ../templates:25
msgid "Action on the chosen free space:"
msgstr "Valitulle vapaalle tilalle tehtävä toimenpide:"

#. Type: select
#. Description
#: ../templates:30
msgid "Action on the partition:"
msgstr "Valitulle osiolle tehtävä toimenpide:"

#. Type: note
#. Description
#: ../templates:34
msgid "The partition starts from ${FROMCHS} and ends at ${TOCHS}."
msgstr "Osion alku: ${FROMCHS} ja loppu: ${TOCHS}."

#. Type: note
#. Description
#: ../templates:38
msgid "The free space starts from ${FROMCHS} and ends at ${TOCHS}."
msgstr "Vapaan tilan alku: ${FROMCHS} ja loppu: ${TOCHS}."

#. Type: text
#. Description
#: ../templates:42
msgid "Please wait..."
msgstr "Odota..."

#. Type: text
#. Description
#: ../templates:46
msgid "Processing..."
msgstr "Käsitellään..."

#. Type: text
#. Description
#: ../templates:58
msgid "Finished with this partition"
msgstr "Tämä osio on valmis"

#. Type: text
#. Description
#: ../templates:62
msgid "Abort the partitioning"
msgstr "Keskeytä osioiden teko"

#. Type: text
#. Description
#: ../templates:66
msgid "Commit the partitioning to the storage devices"
msgstr "Talleta osiot pysyvästi massamuistilaitteille"

#. Type: text
#. Description
#: ../templates:70
msgid "End the partitioning"
msgstr "Lopeta osioiden teko"

#. Type: text
#. Description
#: ../templates:74
msgid "Undo all actions"
msgstr "Kumoa kaikki toimenpiteet"

#. Type: text
#. Description
#: ../templates:78
msgid "Show Cylinder/Head/Sector information"
msgstr "Näytä sylinteri/lukupää/sektori (CHS) -tiedot"

#. Type: text
#. Description
#: ../templates:82
#, no-c-format
msgid "Dump partition info in %s"
msgstr "Osiotiedot tallennetaan paikkaan %s"

#. Type: text
#. Description
#. Keep short
#: ../templates:87
msgid "FREE SPACE"
msgstr "VAPAA TILA"

#. Type: text
#. Description
#. How to print the partition numbers in your language
#. Examples:
#. %s.
#. No %s
#. N. %s
#: ../templates:96
#, no-c-format
msgid "#%s"
msgstr "N:ro %s"

#. Type: text
#. Description
#. Usually IDE0 master or IDE1 master
#: ../templates:101
#, no-c-format
msgid "IDE%s master"
msgstr "IDE%s master"

#. Type: text
#. Description
#. Usually IDE0 slave or IDE1 slave
#: ../templates:106
#, no-c-format
msgid "IDE%s slave"
msgstr "IDE%s slave"

#. Type: text
#. Description
#. For example IDE1 master, partition #5
#: ../templates:111
#, no-c-format
msgid "IDE%s master, partition #%s"
msgstr "IDE%s master, levyosio n:ro %s"

#. Type: text
#. Description
#. For example IDE2 slave, partition #5
#: ../templates:116
#, no-c-format
msgid "IDE%s slave, partition #%s"
msgstr "IDE%s slave, levyosio n:ro %s"

#. Type: text
#. Description
#: ../templates:120
#, no-c-format
msgid "SCSI%s (%s,%s,%s)"
msgstr "SCSI%s (%s,%s,%s)"

#. Type: text
#. Description
#: ../templates:124
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s"
msgstr "SCSI%s (%s,%s,%s), levyosio n:ro %s"

#. Type: text
#. Description
#. Main menu entry
#: ../templates:129
msgid "Partition the storage devices"
msgstr "Tee osiot massamuistilaitteille"

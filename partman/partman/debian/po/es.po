#. Type: select
#. Description
#: ../templates:15
msgid "Partition, free space or device to be modified:"
msgstr "Partición, espacio libre o dispositivo a modificar:"

#. Type: select
#. Description
#: ../templates:20
msgid "Action on the chosen device:"
msgstr "Acción a realizar en el dispositivo elegido:"

#. Type: select
#. Description
#: ../templates:25
msgid "Action on the chosen free space:"
msgstr "Acción en espacio libre elegido:"

#. Type: select
#. Description
#: ../templates:30
msgid "Settings of the partition:"
msgstr "Configuración de la partición:"

#. Type: note
#. Description
#: ../templates:34
msgid "The partition starts from ${FROMCHS} and ends at ${TOCHS}."
msgstr "La partición empieza en ${FROMCHS} y termina en ${TOCHS}."

#. Type: note
#. Description
#: ../templates:38
msgid "The free space starts from ${FROMCHS} and ends at ${TOCHS}."
msgstr "El espacio libre empieza en ${FROMCHS} y termina en ${TOCHS}."

#. Type: text
#. Description
#: ../templates:42
msgid "Please wait..."
msgstr "Espere por favor..."

#. Type: text
#. Description
#: ../templates:46
msgid "Processing..."
msgstr "Procesando..."

#. Type: text
#. Description
#: ../templates:58
msgid "Choose this if you are satisfied with the above settings"
msgstr "Escoja éste si está satisfecho con la configuración mostrada arriba"

#. Type: text
#. Description
#: ../templates:62
msgid "Abort the partitioning (do not write the changes)"
msgstr "Abortar el particionado (no escribir los cambios)"

#. Type: text
#. Description
#: ../templates:66
msgid "Commit the partitioning to the storage devices"
msgstr "Fijar el particionado a los dispositivos de almacenamiento"

#. Type: text
#. Description
#: ../templates:70
msgid "End the partitioning and continue the installation"
msgstr "Terminar el particionado y continuar la instalación"

#. Type: text
#. Description
#: ../templates:74
msgid "Undo all actions"
msgstr "Deshacer todas las acciones"

#. Type: text
#. Description
#: ../templates:78
msgid "Show Cylinder/Head/Sector information"
msgstr "Mostrar información de Cilindros/Cabezas/Sectores"

#. Type: text
#. Description
#: ../templates:82
#, no-c-format
msgid "Dump partition info in %s"
msgstr "Volcar la información de partición en %s"

#. Type: text
#. Description
#. Keep short
#: ../templates:87
msgid "FREE SPACE"
msgstr "ESPACIO LIBRE"

#. Type: text
#. Description
#. How to print the partition numbers in your language
#. Examples:
#. %s.
#. No %s
#. N. %s
#: ../templates:96
#, no-c-format
msgid "#%s"
msgstr "#%s"

#. Type: text
#. Description
#. Usually IDE0 master or IDE1 master
#: ../templates:101
#, no-c-format
msgid "IDE%s master"
msgstr "Maestro IDE%s"

#. Type: text
#. Description
#. Usually IDE0 slave or IDE1 slave
#: ../templates:106
#, no-c-format
msgid "IDE%s slave"
msgstr "Esclavo IDE%s"

#. Type: text
#. Description
#. For example IDE1 master, partition #5
#: ../templates:111
#, no-c-format
msgid "IDE%s master, partition #%s"
msgstr "Maestro IDE%s, partición #%s"

#. Type: text
#. Description
#. For example IDE2 slave, partition #5
#: ../templates:116
#, no-c-format
msgid "IDE%s slave, partition #%s"
msgstr "Esclavo IDE%s, partición #%s"

#. Type: text
#. Description
#: ../templates:120
#, no-c-format
msgid "SCSI%s (%s,%s,%s)"
msgstr "SCSI%s (%s,%s,%s)"

#. Type: text
#. Description
#: ../templates:124
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s"
msgstr "SCSI%s (%s,%s,%s), partición #%s"

#. Type: text
#. Description
#: ../templates:128
msgid "Cancel this menu"
msgstr "Cancelar este menú"

#. Type: text
#. Description
#. Main menu entry
#: ../templates:133
msgid "Partition the storage devices (alternative partitioner)"
msgstr "Particionar los dispositivos de almacenamiento (herramienta de particiones alternativa)"

#. Type: select
#. Description
#: ../templates:15
msgid "Partition, free space or device to be modified:"
msgstr "需要修改的分区、空闲空间或设备："

#. Type: select
#. Description
#: ../templates:20
msgid "Action on the chosen device:"
msgstr "将要在被选设备上执行的动作："

#. Type: select
#. Description
#: ../templates:25
msgid "Action on the chosen free space:"
msgstr "将要在被选空闲空间上执行的动作："

#. Type: select
#. Description
#: ../templates:30
msgid "Action on the chosen partition:"
msgstr "将要在被选分区上执行的动作："

#. Type: note
#. Description
#: ../templates:34
msgid "The partition starts from ${FROMCHS} and ends at ${TOCHS}."
msgstr "此分区的位置开始于 ${FROMCHS} 结束于 ${TOCHS} 。"

#. Type: note
#. Description
#: ../templates:38
msgid "The free space starts from ${FROMCHS} and ends at ${TOCHS}."
msgstr "这块空闲空间的位置开始于 ${FROMCHS} 结束于 ${TOCHS} 。"

#. Type: text
#. Description
#: ../templates:42
msgid "Please wait..."
msgstr "请稍候..."

#. Type: text
#. Description
#: ../templates:46
msgid "Processing..."
msgstr "执行中..."

#. Type: text
#. Description
#: ../templates:58
msgid "Finished with this partition"
msgstr "此分区操作完成"

#. Type: text
#. Description
#: ../templates:62
msgid "Abort the partitioning"
msgstr "中止分区进程"

#. Type: text
#. Description
#: ../templates:66
msgid "Commit the partitioning to the storage devices"
msgstr "将分区信息写入存储设备"

#. Type: text
#. Description
#: ../templates:70
msgid "End the partitioning"
msgstr "分区进程结束"

#. Type: text
#. Description
#: ../templates:74
msgid "Undo all actions"
msgstr "撤消所有动作"

#. Type: text
#. Description
#: ../templates:78
msgid "Show Cylinder/Head/Sector information"
msgstr "显示柱面/磁头/扇区信息"

#. Type: text
#. Description
#: ../templates:82
#, no-c-format
msgid "Dump partition info in %s"
msgstr "将分区信息转储到 %s"

#. Type: text
#. Description
#. Keep short
#: ../templates:87
msgid "FREE SPACE"
msgstr "空闲空间"

#. Type: text
#. Description
#. How to print the partition numbers in your language
#. Examples:
#. %s.
#. No %s
#. N. %s
#: ../templates:96
#, no-c-format
msgid "#%s"
msgstr "#%s"

#. Type: text
#. Description
#. Usually IDE0 master or IDE1 master
#: ../templates:101
#, no-c-format
msgid "IDE%s master"
msgstr "主 IDE 接口下的第 %s 磁盘"

#. Type: text
#. Description
#. Usually IDE0 slave or IDE1 slave
#: ../templates:106
#, no-c-format
msgid "IDE%s slave"
msgstr "从 IDE 接口下的第 %s 磁盘"

#. Type: text
#. Description
#. For example IDE1 master, partition #5
#: ../templates:111
#, no-c-format
msgid "IDE%s master, partition #%s"
msgstr "主 IDE 接口下的第 %s 磁盘, 第 %s 分区"

#. Type: text
#. Description
#. For example IDE2 slave, partition #5
#: ../templates:116
#, no-c-format
msgid "IDE%s slave, partition #%s"
msgstr "从 IDE 接口下的第 %s 磁盘, 第 %s 分区"

#. Type: text
#. Description
#: ../templates:120
#, no-c-format
msgid "SCSI%s (%s,%s,%s)"
msgstr "SCSI%s (%s,%s,%s)"

#. Type: text
#. Description
#: ../templates:124
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s"
msgstr "SCSI%s (%s,%s,%s), 第 %s 分区"

#. Type: text
#. Description
#. Main menu entry
#: ../templates:129
msgid "Partition the storage devices"
msgstr "对此存储设备进行分区"

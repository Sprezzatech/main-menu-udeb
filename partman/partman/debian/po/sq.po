#, no-c-format
msgid "#%s"
msgstr "nr. %s"

#. Type: text
#. Description
#. Usually IDE0 master or IDE1 master
#: ../templates:101
#, no-c-format
msgid "IDE%s master"
msgstr "Master IDE%s"

#. Type: text
#. Description
#. Usually IDE0 slave or IDE1 slave
#: ../templates:106
#, no-c-format
msgid "IDE%s slave"
msgstr "Slave IDE%s"


#. Type: select
#. Description
#: ../templates:15
msgid "Partition, free space or device to be modified:"
msgstr "Partition, freier Speicher oder Gerät, welches bearbeitet werden soll:"

#. Type: select
#. Description
#: ../templates:20
msgid "Action on the chosen device:"
msgstr "Aktion für das gewählte Gerät:"

#. Type: select
#. Description
#: ../templates:25
msgid "Action on the chosen free space:"
msgstr "Aktion für den gewählten freien Speicher:"

#. Type: select
#. Description
#: ../templates:30
msgid "Action on the chosen partition:"
msgstr "Aktion für die gewählte Partition:"

#. Type: note
#. Description
#: ../templates:34
msgid "The partition starts from ${FROMCHS} and ends at ${TOCHS}."
msgstr "Die Partition beginnt bei ${FROMCHS} und endet bei ${TOCHS}."

#. Type: note
#. Description
#: ../templates:38
msgid "The free space starts from ${FROMCHS} and ends at ${TOCHS}."
msgstr "Der freie Speicher beginnt bei ${FROMCHS} und endet bei ${TOCHS}."

#. Type: text
#. Description
#: ../templates:42
msgid "Please wait..."
msgstr "Bitte warten..."

#. Type: text
#. Description
#: ../templates:46
msgid "Processing..."
msgstr "Bearbeite..."

#. Type: text
#. Description
#: ../templates:58
msgid "Finished with this partition"
msgstr "Fertig mit dieser Partition"

#. Type: text
#. Description
#: ../templates:62
msgid "Abort the partitioning"
msgstr "Die Partitionierung abbrechen"

#. Type: text
#. Description
#: ../templates:66
msgid "Commit the partitioning to the storage devices"
msgstr "Partitionierungen auf die Speichergeräte anwenden?"

#. Type: text
#. Description
#: ../templates:70
msgid "End the partitioning"
msgstr "Die Partitionierung beenden"

#. Type: text
#. Description
#: ../templates:74
msgid "Undo all actions"
msgstr "Alle Aktionen rückgängig machen"

#. Type: text
#. Description
#: ../templates:78
msgid "Show Cylinder/Head/Sector information"
msgstr "Anzeigen der Zylinder/Kopf/Sektor Informationen"

#. Type: text
#. Description
#: ../templates:82
#, no-c-format
msgid "Dump partition info in %s"
msgstr "Anzeige der Partitions-Informationen in %s"

#. Type: text
#. Description
#. Keep short
#: ../templates:87
msgid "FREE SPACE"
msgstr "FREIER SPEICHER"

#. Type: text
#. Description
#. How to print the partition numbers in your language
#. Examples:
#. %s.
#. No %s
#. N. %s
#: ../templates:96
#, no-c-format
msgid "#%s"
msgstr "#%s"

#. Type: text
#. Description
#. The decimal point in your language -- dot or comma
#. Escape it with "\"
#: ../templates:102
msgid "\\,"
msgstr "\\,"

#. Type: text
#. Description
#. Usually IDE0 master or IDE1 master
#: ../templates:107
#, no-c-format
msgid "IDE%s master"
msgstr "IDE%s Master"

#. Type: text
#. Description
#. Usually IDE0 slave or IDE1 slave
#: ../templates:112
#, no-c-format
msgid "IDE%s slave"
msgstr "IDE%s Slave"

#. Type: text
#. Description
#. For example IDE1 master, partition #5
#: ../templates:117
#, no-c-format
msgid "IDE%s master, partition #%s"
msgstr "IDE%s Master, Partition #%s"

#. Type: text
#. Description
#. For example IDE2 slave, partition #5
#: ../templates:122
#, no-c-format
msgid "IDE%s slave, partition #%s"
msgstr "IDE%s Slave, Partition #%s"

#. Type: text
#. Description
#: ../templates:126
#, no-c-format
msgid "SCSI%s (%s,%s,%s)"
msgstr "SCSI%s (%s,%s,%s)"

#. Type: text
#. Description
#: ../templates:130
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s"
msgstr "SCSI%s (%s,%s,%s), Partition #%s"


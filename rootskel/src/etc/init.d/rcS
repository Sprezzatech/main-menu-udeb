#!/bin/sh
# Cleanup after the first /linuxrc, which left the initrd mounted
# on /initrd.
umount /initrd/dev
umount /initrd
freeramdisk /dev/rd/0

echo -n "Starting system log daemon: "
/sbin/syslogd -m 0 -O /var/log/syslog
echo -n "syslogd, "
/sbin/klogd
echo "klogd."

echo -n "Calculating module depencies"
depmod -a
echo "."

if [ -d /etc/rcS.d ]; then
    for script in /etc/rcS.d/S[0-9][0-9]*; do
        [ -x "$script" ] && $script
    done
fi

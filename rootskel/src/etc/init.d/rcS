#!/bin/sh
# Cleanup after the first /linuxrc, which left the initrd mounted
# on /initrd.
umount /initrd/dev
umount /initrd
freeramdisk /dev/rd/0

echo -n "Starting system log daemon: "

/sbin/syslogd -m 0
echo "syslogd."

if [ -d /etc/rcS.d ]; then
    for script in /etc/init.d/S[0-9][0-9]*; do
        [ -x "$script" ] && $script
    done
fi

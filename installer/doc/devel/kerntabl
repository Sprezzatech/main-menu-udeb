i386       2.6.32 (-13)     2.6.30-1 (-3)     2.6.32-5
alpha      2.6.30 (-6)      2.6.30-1 (-3)     2.6.30-1
amd64      2.6.32 (-13)     2.6.30-1 (-3)     2.6.32-5
armel      2.6.32 (-13)     2.6.30-2 (-8)     2.6.32-5
hppa       2.6.32 (-9)      2.6.30-1 (-3)     2.6.32-3
ia64       2.6.32 (-9)      2.6.30-1 (-3)     2.6.32-3
m68k       2.6.26-1 (-7)      2.6.21-2 (  ) [1] 2.6.26-1
mips       2.6.32 (-9)      2.6.30-1 (-3)     2.6.32-3
mipsel     2.6.32 (-13)     2.6.30-1 (-3)     2.6.32-5
powerpc    2.6.32 (-9)      2.6.30-1 (-3)     2.6.32-3

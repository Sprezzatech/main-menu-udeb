alpha      2.6.26-1 (-13)     2.6.26-1 (-6)     2.6.26-1
amd64      2.6.30-1 (-3)      2.6.30-1 (-1)     2.6.30-1
armel      2.6.29-2 (-5)      2.6.26-1 (-6)     2.6.29-2
hppa       2.6.29-2 (-4)      2.6.26-1 (-6)     2.6.29-2
ia64       2.6.26-1 (-13)     2.6.26-1 (-6)     2.6.26-1
m68k       2.6.26-1 (-7)      2.6.21-2 (  ) [1] 2.6.26-1
mips       2.6.29-2 (-5)      2.6.26-2 (-4)     2.6.29-2

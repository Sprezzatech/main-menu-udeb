<!-- 
  These are standard URLs for well-known resources on the Internet.
  You can redefine these to your taste in your install.XX.xml.
  The Debian URLs do content negotiation so it's not necessary there.
 -->


<!-- ******************************************
  Standard Debian URLs
  -->

<!ENTITY alioth "alioth.debian.org">
<!ENTITY url-alioth "http://&alioth;/">
<!ENTITY alioth-account-page '<ulink url="https://alioth.debian.org/account">https://alioth.debian.org/account</ulink>'>

<!ENTITY svn-debian-auth "&lt;alioth_username&gt;@svn.debian.org">
<!ENTITY svn-debian "svn.debian.org">

<!ENTITY git-debian-auth "&lt;alioth_username&gt;@git.debian.org">
<!ENTITY git-debian "git.debian.org">

<!ENTITY irc-server "irc.debian.org">
<!ENTITY irc-channel "#debian-boot on &irc-server;">

<!ENTITY backports "backports.debian.org">
<!ENTITY url-backports "http://&backports;/">

<!ENTITY url-status "http://d-i.debian.org/l10n-stats">

<!ENTITY url-languagelist "http://d-i.debian.org/doc/i18n/languages.html">

<!ENTITY url-spellchecker-status "http://d-i.debian.org/l10n-spellcheck/">

<!ENTITY url-d-i-translation-status "&url-status;/translation-status.html">

<!ENTITY url-level1     "&url-status;/level1">
<!ENTITY url-sublevel1-pot     "&url-status;/level1/POT/debian-installer_packages_po_sublevel1.pot">
<!ENTITY url-sublevel2-pot     "&url-status;/level1/POT/debian-installer_packages_po_sublevel2.pot">
<!ENTITY url-sublevel3-pot     "&url-status;/level1/POT/debian-installer_packages_po_sublevel3.pot">
<!ENTITY url-sublevel4-pot     "&url-status;/level1/POT/debian-installer_packages_po_sublevel4.pot">
<!ENTITY url-sublevel5-pot     "&url-status;/level1/POT/debian-installer_packages_po_sublevel5.pot">
<!ENTITY url-sublevel1-po     "&url-status;/level1/files/&language;/debian-installer_packages_po_sublevel1_&language;.po">
<!ENTITY url-sublevel2-po     "&url-status;/level1/files/&language;/debian-installer_packages_po_sublevel2_&language;.po">
<!ENTITY url-sublevel3-po     "&url-status;/level1/files/&language;/debian-installer_packages_po_sublevel3_&language;.po">
<!ENTITY url-sublevel4-po     "&url-status;/level1/files/&language;/debian-installer_packages_po_sublevel4_&language;.po">
<!ENTITY url-sublevel5-po     "&url-status;/level1/files/&language;/debian-installer_packages_po_sublevel5_&language;.po">
<!ENTITY url-sublevel1-pot-link     '<ulink url="&url-sublevel1-pot;">&url-sublevel1-pot;</ulink>'>
<!ENTITY url-sublevel1-po-link     '<ulink url="&url-sublevel1-po;">&url-sublevel1-po;</ulink>'>
<!ENTITY url-sublevel2-pot-link     '<ulink url="&url-sublevel2-pot;">&url-sublevel2-pot;</ulink>'>
<!ENTITY url-sublevel2-po-link     '<ulink url="&url-sublevel2-po;">&url-sublevel2-po;</ulink>'>
<!ENTITY url-sublevel3-pot-link     '<ulink url="&url-sublevel3-pot;">&url-sublevel3-pot;</ulink>'>
<!ENTITY url-sublevel3-po-link     '<ulink url="&url-sublevel3-po;">&url-sublevel3-po;</ulink>'>
<!ENTITY url-sublevel4-pot-link     '<ulink url="&url-sublevel4-pot;">&url-sublevel4-pot;</ulink>'>
<!ENTITY url-sublevel4-po-link     '<ulink url="&url-sublevel4-po;">&url-sublevel4-po;</ulink>'>
<!ENTITY url-sublevel5-pot-link     '<ulink url="&url-sublevel5-pot;">&url-sublevel5-pot;</ulink>'>
<!ENTITY url-sublevel5-po-link     '<ulink url="&url-sublevel5-po;">&url-sublevel5-po;</ulink>'>

<!ENTITY url-level2     "&url-status;/level2/files">
<!ENTITY url-level2-pot     "&url-status;/level2/POT">
<!ENTITY url-level2-po     "&url-status;/level2/files/&language;">
<!ENTITY url-level2-pot-link     '<ulink url="&url-level2-pot;">&url-level2-pot;</ulink>'>
<!ENTITY url-level2-po-link     '<ulink url="&url-level2-po;">&url-level2-po;</ulink>'>

<!ENTITY url-level3     "&url-status;/level3/files">
<!ENTITY url-level3-pot     "&url-status;/level3/POT">
<!ENTITY url-level3-po     "&url-status;/level3/files/&language;">
<!ENTITY url-level3-pot-link     '<ulink url="&url-level3-pot;">&url-level3-pot;</ulink>'>
<!ENTITY url-level3-po-link     '<ulink url="&url-level3-po;">&url-level3-po;</ulink>'>

<!-- SVN/CVS URLs for various files locations -->
<!ENTITY svn-level1 "svn co svn+ssh://&svn-debian-auth;/svn/d-i/trunk/packages/po debian-installer">
<!ENTITY svn-level1-anon "svn co svn+ssh://&svn-debian;/d-i/trunk/packages/po debian-installer">

<!ENTITY svn-di-sandbox "<classname>svn+ssh://&svn-debian-auth;/svn/d-i/people/sandbox</classname>">
<!ENTITY svn-tasksel-anon "svn co svn://&svn-debian;/tasksel/trunk tasksel">
<!ENTITY svn-tasksel "svn co svn+ssh://&svn-debian-auth;/svn/tasksel/trunk tasksel">
<!ENTITY git-tasksel "git clone ssh://git.debian.org/git/tasksel/tasksel.git">
<!ENTITY svn-win32-loader-anon "svn co svn://&svn-debian;/svn/d-i/trunk/win32-loader">
<!ENTITY svn-win32-loader "svn co svn+ssh://&svn-debian-auth;/svn/d-i/trunk/win32-loader">
<!ENTITY git-iso-codes-anon "git clone git://&git-debian;/git/iso-codes/iso-codes.git">
<!ENTITY git-iso-codes "git clone ssh://&git-debian-auth;/git/iso-codes/iso-codes.git">
<!ENTITY svn-console-data "svn co svn://&svn-debian;/console-data/trunk/debian/po console-data">
<!ENTITY svn-grub2 "svn co svn://&svn-debian;/pkg-grub/grub2/trunk/debian/po grub2">
<!ENTITY console-data-en     "&url-status;/level2/files/en/console-data_debian_po_en.po">
<!ENTITY svn-popcon "svn co svn://&svn-debian;/pkg-grub/grub2/trunk/debian/po popularity-contest">
<!ENTITY svn-debconf "svn co svn://&svn-debian;/debconf/trunk/src/debconf debconf">
<!ENTITY git-eject "git clone ssh://git.debian.org/git/collab-maint/eject">

<!-- Wiki page for fonts -->
<!ENTITY wiki-fonts "http://wiki.debian.org/DebianInstaller/GUIFonts">




<!ENTITY url-bts "http://bugs.debian.org/">
<!ENTITY email-bts " <email>submit@bugs.debian.org</email>">

<!ENTITY email-debian-boot-list "<email>debian-boot@lists.debian.org</email>">
<!ENTITY email-debian-i18n-list "<email>debian-i18n@lists.debian.org</email>">

<!ENTITY url-debian-list-archives "http://lists.debian.org/">

<!ENTITY url-debian-l10n "http://www.debian.org/intl/l10n/">


<!-- SVN/CVS URLs for various files locations -->
<!ENTITY svn-level1 "svn co svn+ssh://&svn-debian-auth;/svn/d-i/trunk/packages/po debian-installer">
<!ENTITY svn-level1-anon "svn co svn+ssh://&svn-debian;/d-i/trunk/packages/po debian-installer">

<!ENTITY svn-di-sandbox "<classname>svn+ssh://&svn-debian-auth;/svn/d-i/people/sandbox</classname>">

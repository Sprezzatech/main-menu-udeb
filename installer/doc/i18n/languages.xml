<!-- This file contains information about languages
     supported in the Debian Installer -->

<!-- 
     Data in speakers comes from:
     Gordon, Raymond G., Jr. (ed.), 2005. 
     Ethnologue: Languages of the World, Fifteenth edition. Dallas, Tex.: SIL International. 
     Online version: http://www.ethnologue.com/.
-->

<!-- 
     Data in spearkers_corr are Ethnologue data "corrected" by D-I
     contributors when they feel that Ethnologue is wrong (often by
     minimizing numbers for whatever reason) 
-->

<!-- Some tricks were used for some languages 
     when it comes et number of speakers:

 Ethnologue doesn't have the number for Norwegian Bokmal and Nynorsk,
 so the population of Norway has been divided to both...:-)

 For zh_CN, we use the population of Han Chinese in China. 
 Chinese flavours differ on spoken forms but not on written forms 

 For zh_TW, we use the population of Taiwan. 
-->

<!DOCTYPE language_entries [
	<!ELEMENT language_entries (language_entry+)>
	<!ELEMENT language_entry EMPTY>
	<!ATTLIST language_entry
		code				CDATA	#IMPLIED
		english_name		CDATA	#REQUIRED
		coord_name		CDATA	#REQUIRED
		coord_email		CDATA	#REQUIRED
		coord_gpg		CDATA	#IMPLIED
		coord_account		CDATA	#IMPLIED
		bkp_coord_name		CDATA	#IMPLIED
		bkp_coord_email		CDATA	#IMPLIED
		bkp_coord_gpg		CDATA	#IMPLIED
		bkp_coord_account	CDATA	#IMPLIED
		manual_coord_name	CDATA	#IMPLIED
		manual_coord_email	CDATA	#IMPLIED
		manual_coord_account	CDATA	#IMPLIED
		team_email		CDATA	#IMPLIED
		team_repository		CDATA	#IMPLIED
		supported		( false | true )	#REQUIRED
		nlp_step		( initial | identification | locale | language_name | localechooser_entry | needed_characters | keymap | account | commit | tools | announce | prospective | contact_lost )	#IMPLIED
		speakers		CDATA	#IMPLIED
		speakers_corr		CDATA	#IMPLIED
	>
]>


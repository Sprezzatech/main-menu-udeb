config ARCH i386
config PREBOOT setupqemu
config START_COMMAND startqemu-cdrom
config SHUTDOWN stopqemu cleanupqemu
config CONSOLECOMMAND console-qemu
config BOOT qemu-syslinux-boot
config TEST_1 di-install
config TEST_2 stopqemu startqemu-disk ssh-data-gather
config STAGE_1_MAX_TIME 3600
config STAGE_2_MAX_TIME 600
config USER root
config USER_PASSWORD foo
config QEMU_DISK_SIZE 4G
config QEMU_EXTRA_PARAMS -redir tcp:7001::22
config SSHPORT 7001
config QEMU_SERIAL_PORT 7000
config ISO ~/sid_d-i_etch-i386-netinst.iso

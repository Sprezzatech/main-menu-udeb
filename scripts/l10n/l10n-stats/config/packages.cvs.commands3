svn -q co svn://svn.debian.org/debconf/trunk/src/debconf debconf/ >/dev/null 2>&1
cd debconf/po
make debconf.pot > /dev/null 2>&1
cd ../..


for i in newt ; do
	apt-get $APT_OPT -t unstable source $i >/dev/null
	rm -f *.tar.gz *.dsc *.diff.gz
	if [ ! -d "$i" ] ; then mv $i* $i ; fi
done

rm -rf win32-loader
svn -q co svn://svn.debian.org/svn/d-i/trunk/win32-loader

cd ../


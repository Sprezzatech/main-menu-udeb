#! /usr/bin/make -f

build:

clean:
	dh_testdir
	dh_clean

install: build
	dh_testdir
	dh_testroot
	dh_clean -k

binary-arch: install
	dh_testdir
	dh_testroot
	dh_installdebconf -n
	dh_strip
	dh_compress
	dh_fixperms
	dh_installdeb
	dh_shlibdeps
	dh_di_gencontrol
	dh_di_builddeb

binary: binary-indep binary-arch
.PHONY: clean binary-indep binary-arch binary install

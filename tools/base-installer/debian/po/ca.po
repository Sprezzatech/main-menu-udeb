#. Description
#: ../base-installer.templates:107
msgid "Failed getting Release file ${SUBST0}."
msgstr "No s'ha pogut obtenir el fitxer Release ${SUBST0}."

#. Type: error
#. Description
#: ../base-installer.templates:112
msgid "Invalid Release file: no valid components."
msgstr "El fitxer Release és invàlid: no hi ha components vàlids."

#. Type: error
#. Description
#: ../base-installer.templates:117
msgid "Invalid Release file: no entry for ${SUBST0}."
msgstr "El fitxer Release és invàlid: no hi ha una entrada per a ${SUBST0}."

#. Type: error
#. Description
#: ../base-installer.templates:122
msgid "Couldn't download ${SUBST0}."
msgstr "No s'ha pogut descarregar ${SUBST0}."

#. Type: error
#. Description
#: ../base-installer.templates:127
msgid "Debootstrap Error"
msgstr "Error de Debootstrap"

#. Type: error
#. Description
#: ../base-installer.templates:127
msgid "Interrupt caught ... exiting."
msgstr "S'ha rebut una interrupció... s'està sortint."


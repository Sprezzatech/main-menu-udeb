msgid "Unknown location ${SUBST0}."
msgstr "Неизвестно разположение на ${SUBST0}."

#. Type: error
#. Description
#: ../base-installer.templates:55
msgid "Failed getting Release file ${SUBST0}."
msgstr "Грешка при взимането на файла Release ${SUBST0}."

#. Type: error
#. Description
#: ../base-installer.templates:60
msgid "Invalid Release file, no valid components."
msgstr "Невалиден файл Release, няма валидни компоненти."

#. Type: error
#. Description
#: ../base-installer.templates:65
msgid "Invalid Release file, no entry for ${SUBST0}."
msgstr "Невалиден файл Release, няма ${SUBST0}."

#. Type: error
#. Description
#: ../base-installer.templates:70
msgid "Couldn't download ${SUBST0}."
msgstr "Не може да бъде изтеглен ${SUBST0}."

#. Type: error
#. Description
#: ../base-installer.templates:75
msgid "No ${SUBST0}. Cannot create devices."
msgstr "Няма ${SUBST0}. Не може да бъде създадено устройство."

#. Type: error
#. Description
#: ../base-installer.templates:80
msgid "Unknown dselect method."
msgstr "Неизвестен метод на dselect."

#. Type: error
#. Description
#: ../base-installer.templates:85
msgid "Debootstrap Error"
msgstr "Грешка по време на debootstrap"

#. Type: error
#. Description
#: ../base-installer.templates:85
msgid "Interrupt caught ... exiting."
msgstr "Прихванато е прекъсване... Излизане."


#. Description
#: ../templates:98
msgid "Estimated time left: ${HOURS} hour(s) ${MINUTES} minute(s)"
msgstr "Estimert tid igjen: ${HOURS} time(r) ${MINUTES} minutt(er)"

#. Type: text
#. Description
#: ../templates:102
msgid "Estimated time left: ${MINUTES} minute(s)"
msgstr "Estimert tid igjen: ${MINUTES} minutt(er)"

#. Type: text
#. Description
#: ../templates:106
msgid "Estimated time left: less than 1 minute"
msgstr "Estimert tid igjen: mindre enn ett minutt"


#!/bin/sh
#
# This file should be automatically generated.
#
# Usage:
#   LANGNAME="langname"
#   . languagemap

# The prefered locale (sent to base-config)
LOCALE=C

# The choosen language code
LANGUAGE=C

# A colon separated priority list of language codes and locales to use
# when selecting translations.
LANGUAGELIST=C

# The assumed country (location)
COUNTRY=us

# The prefered mirror code (ftp.code.debian.org
MIRROR=us

# echo "Testing '$LANGNAME'"

case "$LANGNAME" in
	'English (American)')
		LOCALE=en_US
		LANGUAGE=en
		LANGUAGELIST=
		COUNTRY=us
		MIRROR=us
		;;
	'English (British)')
		LOCALE=en_UK
		LANGUAGE=en
		LANGUAGELIST=
		COUNTRY=uk
		MIRROR=uk
		;;
	'Danish')
		LOCALE=da_DK
		LANGUAGE=da
		LANGUAGELIST=
		COUNTRY=dk
		MIRROR=dk
		;;
	'French (France)')
		LOCALE=fr_FR
		LANGUAGE=fr
		LANGUAGELIST=
		COUNTRY=fr
		MIRROR=fr
		;;
	'German (Germany)')
		LOCALE=de_DE
		LANGUAGE=de
		LANGUAGELIST=
		COUNTRY=de
		MIRROR=de
		;;
	'Irish')
		LOCALE=ga_IE
		LANGUAGE=ga
		LANGUAGELIST=
		COUNTRY=ie
		MIRROR=ie
		;;
	'Latvian')
		LOCALE=lv_LV
		LANGUAGE=lv
		LANGUAGELIST=
		COUNTRY=lv
		MIRROR=
		;;
	'Northern Saami (Norway)')
		LOCALE=se_NO
		LANGUAGE=se
		LANGUAGELIST=se:nb:no:nn
		COUNTRY=no
		MIRROR=no
		;;
	'Norwegian Bokmaal')
		LOCALE=no_NO
		LANGUAGE=nb
		LANGUAGELIST=nb:no:nn
		COUNTRY=no
		MIRROR=no
		;;
	'Norwegian Nynorsk')
		LOCALE=nn_NO
		LANGUAGE=nn
		LANGUAGELIST=nn:nb:no
		COUNTRY=no
		MIRROR=no
		;;
	'Swedish (Sweden)')
		LOCALE=sv_SE
		LANGUAGE=sv
		LANGUAGELIST=
		COUNTRY=se
		MIRROR=se
		;;
	*)
		;;
esac

if [ -z "$LANGUAGELIST" ] ; then
    LANGUAGELIST="$LANGUAGE"
fi

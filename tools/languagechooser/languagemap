#!/bin/sh
#
# This file should be automatically generated.
#
# Usage:
#   LANGNAME="langname"
#   . languagemap

# The prefered locale (sent to base-config)
LOCALE=C

# The choosen language code
LANGUAGE=C

# A colon separated priority list of language codes and locales to use
# when selecting translations.
LANGUAGELIST=C

# The assumed country (location)
COUNTRY=us

# The prefered mirror code (ftp.code.debian.org
MIRROR=us

# echo "Testing '$LANGNAME'"

case "$LANGNAME" in
	'English (American)')
		LOCALE=en_US
		LANGUAGE=en
		COUNTRY=US
		MIRROR=us
		;;
	'English (British)')
		LOCALE=en_UK
		LANGUAGE=en
		COUNTRY=UK
		MIRROR=uk
		;;
	'Catalan')
		LOCALE=ca_ES
		LANGUAGE=ca
		LANGUAGELIST=ca:es
		COUNTRY=ES
		MIRROR=
		;;
	'Chinese (Simplified)')
		LOCALE=zh_CN
		LANGUAGE=zh
		LANGUAGELIST=zh
		COUNTRY=CN
		MIRROR=
		;;
	'Chinese (Traditional)')
		LOCALE=zh_TW
		LANGUAGE=zh
		LANGUAGELIST=zh
		COUNTRY=TW
		MIRROR=
		;;
	'Croatian')
		LOCALE=hr_HR
		LANGUAGE=hr
		LANGUAGELIST=hr
		COUNTRY=HR
		MIRROR=
		;;
	'Danish')
		LOCALE=da_DK
		LANGUAGE=da
		COUNTRY=DK
		MIRROR=dk
		;;
	'Dutch')
		LOCALE=nl_NL
		LANGUAGE=nl
		LANGUAGELIST=nl
		# FIXME Dutch also used in Belgium
		COUNTRY=NL
		MIRROR=
		;;
	'Esperanto')
		LOCALE=eo_XX
		LANGUAGE=eo
		LANGUAGELIST=eo
		COUNTRY=
		MIRROR=
		;;
	'French (France)')
		LOCALE=fr_FR
		LANGUAGE=fr
		COUNTRY=FR
		MIRROR=fr
		;;
	'Galician')
		LOCALE=gl_ES
		LANGUAGE=gl
		LANGUAGELIST=gl:es
		COUNTRY=ES
		MIRROR=
		;;
	'German (Germany)')
		LOCALE=de_DE
		LANGUAGE=de
		COUNTRY=DE
		MIRROR=de
		;;
	'Hungarian')
		LOCALE=hu_HU
		LANGUAGE=hu
		COUNTRY=HU
		MIRROR=
		;;
	'Irish')
		LOCALE=ga_IE
		LANGUAGE=ga
		LANGUAGELIST=ga:en_IE
		COUNTRY=IE
		MIRROR=ie
		;;
	'Italian')
		LOCALE=it_IT
		LANGUAGE=it
		COUNTRY=IT
		MIRROR=
		;;
	'Japanese')
		LOCALE=ja_JP
		LANGUAGE=ja
		COUNTRY=JP
		MIRROR=
		;;
	'Korean')
		LOCALE=ko_KR
		LANGUAGE=ko
		#  Could be both KR and KP
		COUNTRY=
		MIRROR=
		;;
	'Latvian')
		LOCALE=lv_LV
		LANGUAGE=lv
		COUNTRY=LV
		MIRROR=
		;;
	'Northern Saami (Norway)')
		LOCALE=se_NO
		LANGUAGE=se
		LANGUAGELIST=se:nb:no:nn
		COUNTRY=no
		MIRROR=no
		;;
	'Norwegian Bokmaal')
		LOCALE=no_NO
		LANGUAGE=nb
		LANGUAGELIST=nb:no:nn:no_NY:da:sv
		COUNTRY=NO
		MIRROR=no
		;;
	'Norwegian Nynorsk')
		LOCALE=nn_NO
		LANGUAGE=nn
		LANGUAGELIST=nn:no_NY:nb:no:da:sv
		COUNTRY=NO
		MIRROR=no
		;;
	'Polish')
		LOCALE=pl_PL
		LANGUAGE=pl
		COUNTRY=PL
		MIRROR=
		;;
	'Portuguese (Brazil)')
		LOCALE=pt_PT
		LANGUAGE=pt
		LANGUAGELIST=pt_BR:pt:pt_PT
		COUNTRY=BR
		MIRROR=
		;;
	'Portuguese (Portugal)')
		LOCALE=pt_PT
		LANGUAGE=pt
		LANGUAGELIST=pt_PR:pt:pt_BR
		COUNTRY=PT
		MIRROR=
		;;
	'Russian')
		LOCALE=ru_RU
		LANGUAGE=ru
		COUNTRY=RU
		MIRROR=
		;;
	'Spanish (Spain)')
		LOCALE=es_ES
		LANUGAGE=es
		COUNTRY=ES
		MIRROR=
		;;
	'Swedish (Sweden)')
		LOCALE=sv_SE
		LANGUAGE=sv
		COUNTRY=SE
		MIRROR=se
		;;
	'Turkish')
		LOCALE=tr_TR
		LANGUAGE=tr
		COUNTRY=TR
		MIRROR=
		;;
	*)
		;;
esac

if [ -z "$LANGUAGELIST" ] ; then
    LANGUAGELIST="$LANGUAGE"
fi

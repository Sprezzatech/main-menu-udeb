" interface      = ${interface}\n"
" hostname       = ${hostname}\n"
" domain         = ${domain}\n"
" dhcp hostname  = ${dhcp_hostname}\n"
" nameservers    = ${nameservers}"
msgstr ""
" interfész       = ${interface}\n"
" gépnév          = ${hostname}\n"
" domain          = ${domain}\n"
" dhcp-gépnév     = ${dhcp_hostname}\n"
" névszolgáltatók = ${nameservers}"


" arayüz          = ${interface}\n"
" makine adı      = ${hostname}\n"
" alan adı        = ${domain}\n"
" ip addresi      = ${ipaddress}\n"
" ağ maskesi      = ${netmask}\n"
" ağ geçidi       = ${gateway}\n"
" pointopoint     = ${pointopoint}\n"
" isim sunucuları = ${nameservers}"


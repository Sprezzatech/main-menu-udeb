" interface      = ${interface}\n"
" hostname       = ${hostname}\n"
" domain         = ${domain}\n"
" dhcp hostname  = ${dhcp_hostname}\n"
" nameservers    = ${nameservers}"
msgstr ""
" interfejs      = ${interface}\n"
" hostname       = ${hostname}\n"
" domena         = ${domain}\n"
" dhcp hostname  = ${dhcp_hostname}\n"
" nameservers    = ${nameservers}"


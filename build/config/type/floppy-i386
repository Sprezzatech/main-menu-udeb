# Other things that need building. Currently i386 only.
EXTRA_FLOPPIES=net_drivers cd_drivers

EXTRA_TARGETS=$(patsubst %,%-stamp, $(EXTRA_FLOPPIES))

EXTRA_IMAGES= $(patsubst %,$(DEST)/%-image.img, $(EXTRA_FLOPPIES))

EXTRA_STATS= $(patsubst %,%-stats,$(EXTRA_FLOPPIES))

DISK_LABEL_net_drivers="Network drivers floppy"
DISK_LABEL_cd_drivers="CD-ROM drivers floppy"

arch-image: $(IMAGE)

$(IMAGE): $(INITRD)
ifeq ($(TYPE),netboot)
	set -e; $(foreach NAME,$(KERNELNAME), \
		tftpboot.sh $(TEMP)/$(NAME) /dev/null $< $@;)
endif
ifeq ($(TYPE),cdrom)
	# This is borken. We have to install arcboot in the CD image.
	set -e; $(foreach NAME,$(KERNELNAME), \
		tftpboot.sh $(TEMP)/$(NAME) /dev/null $< $@;)
endif
